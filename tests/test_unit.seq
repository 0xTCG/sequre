from sequre.mpc.env import MPCEnv


def test_unit[TP](mpc: MPCEnv[TP]):
    from unit_tests.test_primitives import test_primitives
    test_primitives(mpc)
    
    from unit_tests.test_mpc import test_mpc
    test_mpc(mpc)
    
    from unit_tests.test_sequre_stdlib import test_sequre_stdlib
    test_sequre_stdlib(mpc)

    print(f'All substantial tests passed at {mpc.pid}!\n')
