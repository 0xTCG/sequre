from utils import assert_approx 

from sequre.utils.param import *
from sequre.mpc.env import MPCEnv
from sequre.types.shared_tensor import MODULUS
from ..protocols.sequre_gwas import gwas_protocol


def test_sequre_gwas[TP](mpc: MPCEnv[TP]):
    with open('tests/data/output/gwas.txt') as f:
        expected_output = [float(e.strip()) for e in next(f).split(',')]
        gwas_result = mpc.comms.print_fp(gwas_protocol(mpc, True).share, MODULUS)
        if mpc.pid:
            assert_approx('Sequre GWAS', gwas_result, expected_output, error=0.04)

    print(f'Sequre GWAS passed at {mpc.pid}!\n')
