from utils import assert_approx 

from ..utils.param import *
from ..mpc.env import MPCEnv
from ..protocol.sequre_gwas import gwas_protocol


def test_optimized_gwas[TP](mpc: MPCEnv[TP]):
    with open('tests/data/output/gwas.txt') as f:
        expected_output = [float(e.strip()) for e in next(f).split(',')]
        gwas_result = mpc.comms.print_fp(gwas_protocol(mpc, True).share)
        if mpc.pid:
            assert_approx('Optimized GWAS', gwas_result, expected_output, error=0.04)

    print(f'Optimized GWAS passed at {mpc.pid}!\n')
