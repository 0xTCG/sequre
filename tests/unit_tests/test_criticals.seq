from sequre.types.utils import double_to_fp
from sequre.utils.param import FIELD_SIZE


def test_criticals(mpc, modulus = FIELD_SIZE):
    fat_mat = [[double_to_fp(float((i + j) % 7), modulus) for j in range(1000)] for i in range(15)]
    mpc.lin_alg.orthonormal_basis(fat_mat, modulus)

    print f'Critical tests passed at CP{mpc.pid}.'
