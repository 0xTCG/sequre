@sequre
def test_ciphertensor_lazy_rescaling(mpc, rows, cols):
    mpc.stats.reset_security_stats()
    with mpc.randomness.seed_switch(-1):
        values = random_floats((rows, cols))
    
    if mpc.pid > 0:
        encrypted_values = CipherTensor[ckks.Ciphertext].enc(mpc, values)
        ones = CipherTensor.zeros(mpc, [rows, cols]) + 1

        for i in range(10):
            encrypted_values *= ones
            assert_eq(f"CP{mpc.pid}:\tSequre MHE ciphertensor lazy rescaling for shape ({rows}, {cols}) after {i + 1} muls: stats", mpc.stats.secure_rescale_count, i * len(encrypted_values._data))

        precision_stats = mpc.he.precision_stats(
            result=encrypted_values.reveal(mpc, T=float).tolist(),
            expected=values)
        
        assert_eq(f"CP{mpc.pid}:\tSequre MHE ciphertensor lazy rescaling for shape ({rows}, {cols}): correctness", precision_stats.min_precision.l2 > 7, True)


@sequre
def test_ciphertensor_lazy_bootstrapping(mpc, rows, cols):
    mpc.stats.reset_security_stats()
    with mpc.randomness.seed_switch(-1):
        values = random_floats((rows, cols))
    
    if mpc.pid > 0:
        encrypted_values = CipherTensor[ckks.Ciphertext].enc(mpc, values)
        initial_level = encrypted_values.level()
        ones = CipherTensor.zeros(mpc, [rows, cols]) + 1

        for _ in range(initial_level - mpc.he.bootstrap_min_level + 1):
            encrypted_values *= ones

        precision_stats = mpc.he.precision_stats(
            result=encrypted_values.reveal(mpc, T=float).tolist(),
            expected=values)
        
        assert_eq(f"CP{mpc.pid}:\tSequre MHE ciphertensor lazy bootstrapping for shape ({rows}, {cols}): correctness", precision_stats.min_precision.l2 > 8, True)
        assert_eq(f"CP{mpc.pid}:\tSequre MHE ciphertensor lazy bootstrapping for shape ({rows}, {cols}): level", encrypted_values.level(), initial_level)
        assert_eq(f"CP{mpc.pid}:\tSequre MHE ciphertensor lazy bootstrapping for shape ({rows}, {cols}): stats", mpc.stats.secure_bootstrap_count, len(encrypted_values._data))
    
    # Edge case (fishing for potentially unnecessary bootstraps)
    mpc.stats.reset_security_stats()
    with mpc.randomness.seed_switch(-1):
        values = random_floats((4, 4))
        fat_values = random_floats((4, 32))

    if mpc.pid > 0:
        encrypted_values = CipherTensor[ckks.Ciphertext].enc(mpc, values)

        power_matrix = encrypted_values.copy()
        for _ in range(2):
            power_matrix @= encrypted_values.T
        
        assert_eq(f"CP{mpc.pid}:\tSequre MHE ciphertensor lazy bootstrapping (fishing) for shape ({rows}, {cols}): stats", mpc.stats.secure_bootstrap_count, 16)
        power_matrix = power_matrix.T @ CipherTensor[ckks.Ciphertext].enc(mpc, fat_values)
        assert_eq(f"CP{mpc.pid}:\tSequre MHE ciphertensor lazy bootstrapping (fishing) for shape ({rows}, {cols}): stats", mpc.stats.secure_bootstrap_count, 24)

