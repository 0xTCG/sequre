from test_field_mpc import test_field_mpc, FIELD_SIZE
from test_ring_mpc import test_ring_mpc, RING_SIZE
from test_criticals import test_criticals


def test_mpc(mpc):
    test_criticals(mpc, FIELD_SIZE)
    test_criticals(mpc, RING_SIZE)
    test_field_mpc(mpc)
    test_ring_mpc(mpc)

    print(f'MPC tests passed at {mpc.pid}!\n')
