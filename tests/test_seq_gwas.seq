from utils import assert_values, assert_approx 

from sequre.utils.param import *
from sequre.mpc.env import MPCEnv
from ..protocols.seq_gwas import gwas_protocol


def test_seq_gwas[TP](mpc: MPCEnv[TP], modulus = FIELD_PRIME):
    with open('tests/data/output/gwas.txt') as f:
        expected_output = [float(e.strip()) for e in next(f).split(',')]
        gwas_result = mpc.comms.print_fp(gwas_protocol(mpc, True, modulus), modulus)
        if mpc.pid:
            assert_approx('Seq GWAS', gwas_result, expected_output, error=0.04)
    
    print(f'Seq GWAS passed at {mpc.pid}!\n')
