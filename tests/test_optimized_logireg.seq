from utils import assert_values, assert_approx 

from ..utils.param import *
from ..mpc.env import MPCEnv
from ..protocol.sequre_logireg import logireg_protocol


def test_optimized_logireg[TP](mpc: MPCEnv[TP]):
    with open('tests/data/output/logireg.txt') as f:
        expected_output = [float(e.strip()) for e in next(f).split(' ')]
        gwas_result = mpc.comms.print_fp(logireg_protocol(mpc, True).share)
        if mpc.pid:
            assert_approx('Optimized logistic regression', gwas_result, expected_output, error=0.02)

    print(f'Optimized logistic regression passed at {mpc.pid}!\n')
