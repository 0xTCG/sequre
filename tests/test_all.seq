from sequre.mpc.env import MPCEnv


def test_all[TP](mpc: MPCEnv[TP]):
    from test_primitives import test_primitives
    test_primitives(mpc)
    
    from test_mpc import test_mpc
    test_mpc(mpc)
    
    from test_sequre_stdlib import test_sequre_stdlib
    test_sequre_stdlib(mpc)

    from test_seq_gwas import test_seq_gwas
    test_seq_gwas(mpc)

    from test_sequre_gwas import test_sequre_gwas
    test_sequre_gwas(mpc)

    from test_seq_logireg import test_seq_logireg
    test_seq_logireg(mpc)

    from test_sequre_logireg import test_sequre_logireg
    test_sequre_logireg(mpc)

    from test_seq_nn import test_seq_nn
    test_seq_nn(mpc)

    # from test_sequre_nn import test_sequre_nn
    # test_sequre_nn(mpc)

    print(f'All tests passed at {mpc.pid}!\n')
