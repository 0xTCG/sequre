from ..utils import assert_approx 

from sequre.utils.param import *
from sequre.mpc.env import MPCEnv
from .. import logireg_protocol


def test_sequre_logireg[TP](mpc: MPCEnv[TP], modulus: TP):
    with open('tests/data/output/logireg.txt') as f:
        expected_output = [float(e.strip()) for e in next(f).split(' ')]
        gwas_result = mpc.comms.print_fp(logireg_protocol(mpc, True, modulus).share, modulus)
        if mpc.pid:
            assert_approx(f'Sequre logistic regression ({"ring" if modulus.popcnt() == 1 else "field"})', gwas_result, expected_output, error=0.02)

    print(f'Sequre logistic regression ({"ring" if modulus.popcnt() == 1 else "field"}) passed at {mpc.pid}!\n')
