    print f"End-to-end tests initiated for {'ring' if modulus.popcnt() == 1 else 'field'} at CP{mpc.pid}"

    from test_sequre_gwas import test_sequre_gwas
    test_sequre_gwas(mpc, modulus)

    from test_sequre_logireg import test_sequre_logireg
    test_sequre_logireg(mpc, modulus)

    from test_sequre_nn import test_sequre_nn
    test_sequre_nn(mpc, modulus)

    from test_opal import test_opal
    test_opal(mpc, modulus)

