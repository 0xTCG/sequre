from ..utils import assert_approx 

from sequre.mpc.env import MPCEnv
from .. import seq_logireg_protocol


def test_seq_logireg[TP](mpc: MPCEnv[TP], modulus: TP):
    with open('tests/data/output/logireg.txt') as f:
        expected_output = [float(e.strip()) for e in next(f).split(' ')]
        logireg_result_field = mpc.comms.print_fp(seq_logireg_protocol(mpc, True, modulus), modulus)
        if mpc.pid: assert_approx(f'Seq logistic regression ({"ring" if modulus.popcnt() == 1 else "field"})', logireg_result_field, expected_output, error=0.02)

    print(f'Seq logistic regression ({"ring" if modulus.popcnt() == 1 else "field"}) passed at {mpc.pid}!\n')
