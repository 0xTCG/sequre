from ..utils import assert_approx 

from sequre.utils.param import *
from sequre.mpc.env import MPCEnv
from .. import sequre_neural_net_protocol


def test_sequre_nn[TP](mpc: MPCEnv[TP], modulus: TP):
    with open('tests/data/output/neural_net.txt') as f:
        expected_output = [[float(e.strip()) for e in row.split(' ')] for row in f]
        nn_result = mpc.comms.print_fp(sequre_neural_net_protocol(mpc, True, modulus).share, modulus)
        if mpc.pid:
            assert_approx(f'Sequre neural net ({"ring" if modulus.popcnt() == 1 else "field"})', nn_result, expected_output, error=1)

    print(f'Sequre neural net ({"ring" if modulus.popcnt() == 1 else "field"}) passed at {mpc.pid}!\n')
