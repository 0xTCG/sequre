from utils import assert_values, assert_approx 

from sequre.utils.param import *
from sequre.mpc.env import MPCEnv
from ..protocols.gwas import gwas_protocol


def test_gwas[TP](mpc: MPCEnv[TP]):
    with open('tests/data/output/gwas.txt') as f:
        expected_output = [float(e.strip()) for e in next(f).split(',')]
        gwas_result = mpc.comms.print_fp(gwas_protocol(mpc, True))
        if mpc.pid:
            assert_approx('GWAS', gwas_result, expected_output, error=0.04)
    
    print(f'GWAS passed at {mpc.pid}!\n')
