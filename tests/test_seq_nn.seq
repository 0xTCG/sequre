from utils import assert_approx 

from sequre.utils.param import *
from sequre.mpc.env import MPCEnv
from ..protocols.seq_nn import neural_net_protocol


def test_seq_logireg[TP](mpc: MPCEnv[TP]):
    with open('tests/data/output/neural_net.txt') as f:
        expected_output = [float(e.strip()) for e in next(f).split(' ')]
        nn_result = mpc.comms.print_fp(neural_net_protocol(mpc, True))
        if mpc.pid:
            assert_approx('Seq neural net', nn_result, expected_output, error=0.02)

    print(f'Seq neural net passed at {mpc.pid}!\n')
