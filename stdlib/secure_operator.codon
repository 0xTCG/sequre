from sequre.attributes import sequre


@sequre
def add(mpc, a, b):
    return a + b
    

@sequre
def sub(mpc, a, b):
    return a - b


@sequre
def mul(mpc, a, b):
    return a * b


@sequre
def matmul(mpc, a, b):
    return a @ b


@sequre
def pow(mpc, a, p: int):
    return a ** p


@sequre
def lt(mpc, a, b):
    return a < b


@sequre
def gt(mpc, a, b):
    return a > b


def identity(whatever):
    return whatever
