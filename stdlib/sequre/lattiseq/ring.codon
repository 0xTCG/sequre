# BRedParams computes the parameters for the BRed algorithm.
# Returns ((2^128)/q)/(2^64) and (2^128)/q mod 2^64.
def bred_params(q: u64):
    if q.popcnt() == 1:
        d = UInt[128](1) << (128 - int(q.bitlen()) + 1)
    else:
        d = UInt[128](-1) // q.ext_double()
    
    mhi = (d >> 64).trunc_half()
    mlo = d.trunc_half()

    return [mhi, mlo]


# BRed computes x*y mod q.
def bred(x, y, q, u):
    mhi, mlo = u64(x).mul_overflow_u64(u64(y))
    r = mhi * u[0]
    hhi, hlo = mlo.mul_overflow_u64(u[0])
    r += hhi
    lhi, _ = mlo.mul_overflow_u64(u[1])
    s0, carry = hlo.add_overflow(lhi)
    r += carry
    hhi, hlo = mhi.mul_overflow_u64(u[1])
    r += hhi
    _, carry = hlo.add_overflow(s0)
    r += carry
    r = mlo - r * q
    if r >= q: r -= q
    return r


# ModExp performs the modular exponentiation x^e mod p,
# x and p are required to self at most 64 bits to avoid an overflow.
def mod_exp_u64(x, e, p):
    params = bred_params(p)
    result = u64(1)

    while e:
        if e & type(e)(1) == type(e)(1): result = bred(result, x, p, params)
        x = type(x)(bred(x, x, p, params))
        e >>= type(e)(1)

    return result


# MForm switches a to the Montgomery domain by computing
# a*2^64 mod q.
def mform(a, q, u):
    mhi, _ = a.mul_overflow_u64(u[1])
    r = -(a * u[0] + mhi) * q
    if r >= q: r -= q
    return r


# MForm switches a to the Montgomery domain by computing
# a*2^64 mod q.
def _mm_mform(a, q, u):
    mhi = a.mulhi(u[1])
    r = -(a * u[0] + mhi) * q
    return r.sub_if(q, r.__ge__(q))


# InvMForm switches a from the Montgomery domain back to the
# standard domain by computing a*(1/2^64) mod q.
def _mm_inv_mform(a: u64xN, q: u64xN, q_inv: u64xN) -> u64xN:
	r = (a * q_inv).mulhi(q)
	r, c = q.sub_overflow(r)
	r = r.sub_if(q, c)
	return r


# MRedParams computes the parameter q_inv = (q^-1) mod 2^64,
# required for mred.
def mred_params(q):
	q_inv = u64(1)
	for _ in range(63):
		q_inv *= q
		q *= q

	return q_inv


# mred computes x * y * (1/2^64) mod q.
def mred(x, y, q, q_inv):
    mhi, mlo = x.mul_overflow_u64(y)
    hhi, _ = (mlo * q_inv).mul_overflow_u64(q)
    r = mhi - hhi + q
    if r >= q: r -= q
    return r


# mred computes x * y * (1/2^64) mod q.
def _mm_mred(x, y, q, q_inv):
    m = x.zext_mul(y)
    h = (m.trunc_half() * q_inv).mulhi(q)
    r, c = (m.shift_half().trunc_half() - h).add_overflow(q)
    return r.sub_if(q, c.bit_flip())


# CRed reduce returns a mod q where a is between 0 and 2*q-1.
def cred(a, q):
    if a >= q: return a - q
    return a


# CRed reduce returns a mod q where a is between 0 and 2*q-1.
def _mm_cred(a, q):
    return a.sub_if(q, a.__ge__(q))


# mred_constant computes x * y * (1/2^64) mod q in constant time.
# The result is between 0 and 2*q-1.
def mred_constant(x, y, q, q_inv):
    ahi, alo = x.mul_overflow_u64(y)
    h, _ = (alo * q_inv).mul_overflow_u64(q)
    return ahi - h + q


def _mm_mred_constant(x: u64xN, y: u64xN, q: u64xN, q_inv: u64xN) -> u64xN:
    a = x.zext_mul(y)
    h = (a.trunc_half() * q_inv).mulhi(q)
    return a.shift_half().trunc_half() - h + q


def _mm_butterfly(u: u64xN, v: u64xN, psi: u64xN, two_q: u64xN, four_q: u64xN, q: u64xN, q_inv: u64xN) -> Tuple[u64xN, u64xN]:
    u = u.sub_if(four_q, u.__ge__(four_q))
    v = _mm_mred_constant(v, psi, q, q_inv)
    return u + v, u + two_q - v


@llvm
def _mm_butterfly_inplace_x2_sub(u: u64xN, four_q: u64xN) -> u64xN:
    %0 = icmp uge <4 x i64> %u, %four_q
    %1 = and <4 x i1> %0, <i1 1, i1 1, i1 0, i1 0>
    %2 = sub <4 x i64> %u, %four_q
    %3 = select <4 x i1> %1, <4 x i64> %2, <4 x i64> %u
    ret <4 x i64> %3


@llvm
def _mm_butterfly_inplace_x2_shuffle_addend_1(u: u64xN, two_q: u64xN) -> u64xN:
    %0 = shufflevector <4 x i64> %u, <4 x i64> undef, <4 x i32> <i32 0, i32 1, i32 0, i32 1>
    %1 = add <4 x i64> %0, %two_q
    ret <4 x i64> %1


@llvm
def _mm_butterfly_inplace_x2_shuffle_addend_2(u: u64xN) -> u64xN:
    %0 = shufflevector <4 x i64> %u, <4 x i64> undef, <4 x i32> <i32 2, i32 3, i32 2, i32 3>
    ret <4 x i64> %0


@llvm
def _mm_butterfly_inplace_x2_twist(u: u64xN) -> u64xN:
    %0 = sub <4 x i64> zeroinitializer, %u
    %1 = shufflevector <4 x i64> %u, <4 x i64> %0, <4 x i32> <i32 0, i32 1, i32 6, i32 7>
    ret <4 x i64> %1


def _mm_ntt_intrinsic_inplace(coeffs: u64xN, ntt_psi: u64xN, two_q: u64xN, q: u64xN, q_inv: u64xN) -> u64xN:
    res = _mm_butterfly_inplace_x2_shuffle_addend_1(coeffs, two_q)
    mrc = _mm_mred_constant(_mm_butterfly_inplace_x2_shuffle_addend_2(coeffs), ntt_psi, q, q_inv)
    alt_mrc = _mm_butterfly_inplace_x2_twist(mrc)
    return res + alt_mrc


def _mm_butterfly_inplace_x2(u: u64xN, ntt_psi: u64xN, two_q: u64xN, four_q: u64xN, q: u64xN, q_inv: u64xN) -> u64xN:
    u = _mm_butterfly_inplace_x2_sub(u, four_q)
    return _mm_ntt_intrinsic_inplace(u, ntt_psi, two_q, q, q_inv)


@llvm
def _mm_butterfly_inplace_x1_sub(u: u64xN, four_q: u64xN) -> u64xN:
    %0 = icmp uge <4 x i64> %u, %four_q
    %1 = and <4 x i1> %0, <i1 1, i1 0, i1 1, i1 0>
    %2 = sub <4 x i64> %u, %four_q
    %3 = select <4 x i1> %1, <4 x i64> %2, <4 x i64> %u
    ret <4 x i64> %3


@llvm
def _mm_butterfly_inplace_x1_shuffle_addend_1(u: u64xN, two_q: u64xN) -> u64xN:
    %0 = shufflevector <4 x i64> %u, <4 x i64> undef, <4 x i32> <i32 0, i32 0, i32 2, i32 2>
    %1 = add <4 x i64> %0, %two_q
    ret <4 x i64> %1


@llvm
def _mm_butterfly_inplace_x1_shuffle_addend_2(u: u64xN) -> u64xN:
    %0 = shufflevector <4 x i64> %u, <4 x i64> undef, <4 x i32> <i32 1, i32 1, i32 3, i32 3>
    ret <4 x i64> %0


@llvm
def _mm_butterfly_inplace_x1_twist(u: u64xN) -> u64xN:
    %0 = sub <4 x i64> zeroinitializer, %u
    %1 = shufflevector <4 x i64> %u, <4 x i64> %0, <4 x i32> <i32 0, i32 5, i32 2, i32 7>
    ret <4 x i64> %1


def _mm_butterfly_inplace_x1(u: u64xN, ntt_psi: u64xN, two_q: u64xN, four_q: u64xN, q: u64xN, q_inv: u64xN) -> u64xN:
    u = _mm_butterfly_inplace_x1_sub(u, four_q)
    res = _mm_butterfly_inplace_x1_shuffle_addend_1(u, two_q)
    mrc = _mm_mred_constant(_mm_butterfly_inplace_x1_shuffle_addend_2(u), ntt_psi, q, q_inv)
    alt_mrc = _mm_butterfly_inplace_x1_twist(mrc)
    return res + alt_mrc

                
# invbutterfly computes X, Y = U + v, (U - v) * Psi mod q.
def invbutterfly(u, v, psi, two_q, four_q, q, q_inv):
    x = u + v
    if x >= two_q: x -= two_q
    # At the moment it is not possible to use mred_constant if q > 61 bits
    y = mred_constant(u + four_q - v, psi, q, q_inv)
    return x, y


# invbutterfly computes X, Y = U + v, (U - v) * Psi mod q.
def _mm_invbutterfly(u, v, psi, two_q, four_q, q, q_inv):
    x = u + v
    x = x.sub_if(two_q, x.__ge__(two_q))
    # At the moment it is not possible to use mred_constant if q > 61 bits
    y = _mm_mred_constant(u + four_q - v, psi, q, q_inv)
    return x, y


# bred_add computes a mod q.
def bred_add(a: u64, q: u64, u0: u64):
    mhi, _ = a.mul_overflow_u64(u0)
    r = a - mhi * q
    if r >= q: r -= q
    return r


# bred_add computes a mod q.
def _mm_bred_add(a: u64xN, q: u64xN, u0: u64xN):
    r = a - a.mulhi(u0) * q
    return r.sub_if(q, r.__ge__(q))


# ModexpMontgomery performs the modular exponentiation x^e mod p,
# where x is in Montgomery form, and returns x^e in Montgomery form.
def mod_exp_montgomery(x, e, q, q_inv, bred_params):
    result = mform(u64(1), q, bred_params)

    while e:
        if e & 1 == 1: result = mred(result, x, q, q_inv)
        x = mred(x, x, q, q_inv)
        e >>= 1
    
    return result


# gcd computes the greatest common divisor gcd(a,b) for a,b u64 variables
def gcd(a, b):
    if a == u64(0) or b == u64(0): return u64(0)
    while b != u64(0): a, b = b, a.__naive_mod(b)

    return a


# polynomialPollardsRho calculates x1^2 + c mod x2, and is used in factorizationPollardsRho
def polynomial_pollards_rho(x1, x2, c):
    z = mod_exp_u64(x1, 2, x2)  # x1^2 mod x2
    z += u64(c)  # (x1^2 mod x2) + 1
    z = z.__naive_mod(x2)  # (x1^2 + 1) mod x2
    return z


# factorizationPollardsRho realizes Pollard's Rho algorithm for fast prime factorization,
# but this function only returns one factor per call
def factorization_pollards_rho(m):
    # c is used to change the ring in Pollard's Rho algorithm,
    # Every time the algorithm fails to get a factor, c is increased and a retry starts,
    # because Pollard's Rho algorithm sometimes will miss some small prime factors.
    for c in range(1, 10):
        x, y, d = u64(2), u64(2), u64(1)

        while d != u64(0):
            # Walk, walk and eventually meet \o/
            x = polynomial_pollards_rho(x, m, c)
            y = polynomial_pollards_rho(polynomial_pollards_rho(y, m, c), m, c)

            if y > x:  # swap to avoid overflow
                x, y = y, x

            d = gcd(x - y, m)

            if d > u64(1): return d

    return u64(0)


# getFactors returns all the prime factors of m.
def get_factors(n):
    m = n
    factors = []

    # first, append small prime factors
    for i in range(len(SMALL_PRIMES)):
        small_prime = u64(SMALL_PRIMES[i])
        add_factor = False
        while m.__naive_mod(small_prime) == u64(0):
            m //= small_prime
            add_factor = True

        if add_factor: factors.append(small_prime)

    if m == u64(1): return factors

	# second, find other prime factors
    while True:
        factor = factorization_pollards_rho(m)
        if factor == u64(0):
            factors.append(m)
            break

        m //= factor
        if len(factors) > 0 and factor == factors[len(factors) - 1]: continue
        factors.append(factor)

    return factors


# primitiveRoot computes one primitive root (the smallest) of for the given prime q
def primitive_root(q):
    not_found_primitive_root = True

    factors = get_factors(q - u64(1)) # Factor q-1, might self slow

    g = 2

    while not_found_primitive_root:
        g += 1
        for factor in factors:
            # if for any factor of q-1, g^(q-1)/factor = 1 mod q, g is not a primitive root
            if mod_exp_u64(g, (q - u64(1)) // factor, q) == u64(1):
                not_found_primitive_root = True
                break
            not_found_primitive_root = False
    
    return g


# AddScalarVec returns p2 = p1 + scalar mod qi.
def _mm_add_scalar_vec(p1: list[u64xN], p2: list[u64xN], scalar: u64xN, qi: u64xN):
	for j in range(len(p1)):
		p2[j] = _mm_cred(p1[j] + scalar, qi)


# AddScalarNoModAndNegTwoQiNoModVec returns p2 = 2*qi - p1 + scalar.
def _mm_add_scalar_no_mod_and_neg_two_qi_no_mod_vec(p1: list[u64xN], p2: list[u64xN], scalar: u64xN, qi: u64xN):
	twoqi = qi << u64xN(u64(1))
	for j in range(len(p1)):
		p2[j] = scalar + twoqi - p1[j]


# AddScalarNoModVec returns p2 = p1 + scalar.
def _mm_add_scalar_no_mod_vec(p1: list[u64xN], p2: list[u64xN], scalar: u64xN):
	for j in range(len(p1)):
		p2[j] = p1[j] + scalar


# AddVecNoModAndMulScalarMontgomeryVec returns p3 = (p1+p2)*scalar_mont mod qi.
def _mm_add_vec_no_mod_and_mul_scalar_montgomery_vec(p1: list[u64xN], p2: list[u64xN], p3: list[u64xN], scalar_mont: u64xN, qi: u64xN, mred_params: u64xN):
	for j in range(len(p1)):
		p3[j] = _mm_mred(p1[j] + p2[j], scalar_mont, qi, mred_params)


# ReduceVec returns p2 = p1 mod qi.
def _mm_reduce_vec(p1, p2, qi, bred_params):
    for j in range(len(p1)):
        p2[j] = _mm_bred_add(p1[j], qi, bred_params)


# MFormVec returns p2 = p1 * 2^64 mod qi.
def mform_vec(p1, p2, qi, bred_params):
	for j in range(len(p1)):
		p2[j] = mform(p1[j], qi, bred_params)


# MFormVec returns p2 = p1 * 2^64 mod qi.
def _mm_mform_vec(p1, p2, qi, bred_params):
	for j in range(len(p1)):
		p2[j] = _mm_mform(p1[j], qi, bred_params)


# InvMFormVec returns p2 = p1 * (2^64)^-1 mod qi.
def _mm_inv_mform_vec(p1: list[u64xN], p2: list[u64xN], qi: u64xN, mred_params: u64xN):
	for j in range(len(p1)):
		p2[j] = _mm_inv_mform(p1[j], qi, mred_params)


# MulScalarMontgomeryVec returns p2 = p1*scalar_mont mod qi.
def _mm_mul_scalar_montgomery_vec(p1, p2, scalar_mont, qi, mred_params):
	for j in range(len(p1)):
		p2[j] = _mm_mred(p1[j], scalar_mont, qi, mred_params)


# MulScalarMontgomeryVec returns p2 = p1*scalar_mont mod qi.
def _mm_mul_scalar_montgomery_vec(p1, p2, scalar_mont, qi, mred_params):
	for j in range(len(p1)):
		p2[j] = _mm_mred(p1[j], scalar_mont, qi, mred_params)


# mul_scalar_montgomery_constant_vec returns p2 = p1*scalar_mont mod qi with output coefficients in range [0, 2qi-1].
def _mm_mul_scalar_montgomery_constant_vec(p1: list[u64xN], p2: list[u64xN], scalar_mont: u64xN, qi: u64xN, mred_params: u64xN):
	for j in range(len(p1)):
		p2[j] = _mm_mred_constant(p1[j], scalar_mont, qi, mred_params)


# MulcoeffsMontgomeryVec returns p3 = p1*p2 mod qi.
def _mm_mul_coeffs_montgomery_vec(p1, p2, p3, qi, mred_params):
    for j in range(len(p1)):
        p3[j] = _mm_mred(p1[j], p2[j], qi, mred_params)


# MulcoeffsMontgomeryAndSubVec returns p3 = p3 - p1*p2 mod qi.
def _mm_mul_coeffs_montgomery_and_sub_vec(p1, p2, p3, qi, mred_params):
	for j in range(len(p1)):
		p3[j] = _mm_cred(p3[j] + (qi - _mm_mred(p1[j], p2[j], qi, mred_params)), qi)


# MulcoeffsMontgomeryConstantVec returns p3 = p1*p2 mod qi with output coefficients in range [0, 2qi-1].
def _mm_mul_coeffs_montgomery_constant_vec(p1: list[u64xN], p2: list[u64xN], p3: list[u64xN], qi: u64xN, mred_params: u64xN):
	for j in range(len(p1)):
		p3[j] = _mm_mred_constant(p1[j], p2[j], qi, mred_params)


# MulcoeffsMontgomeryConstantAndAddNoModVec returns p3 = p3 + p1*p2 mod qi with output coefficients in range [0, 3qi-2].
def _mm_mul_coeffs_montgomery_constant_and_add_no_mod_vec(p1: list[u64xN], p2: list[u64xN], p3: list[u64xN], qi: u64xN, mred_params: u64xN):
	for j in range(len(p1)):
		p3[j] += _mm_mred_constant(p1[j], p2[j], qi, mred_params)


# MulcoeffsMontgomeryAndAddVec returns p3 = p3 + (p1*p2) mod qi.
def _mm_mul_coeffs_montgomery_and_add_vec(p1: list[u64xN], p2: list[u64xN], p3: list[u64xN], qi: u64xN, mred_params: u64xN):
	for j in range(len(p1)):
		p3[j] = _mm_cred(p3[j] + _mm_mred(p1[j], p2[j], qi, mred_params), qi)


# sub_vec_and_mul_scalar_montgomery_two_qi_vec returns p3 = (p1 + twoqi - p2) * scalar_mont mod qi.
def _mm_sub_vec_and_mul_scalar_montgomery_two_qi_vec(p1: list[u64xN], p2: list[u64xN], p3: list[u64xN], scalar_mont: u64xN, qi: u64xN, mred_params: u64xN):
	twoqi = qi << u64xN(u64(1))
	for j in range(len(p1)):
		p3[j] = _mm_mred(twoqi - p2[j] + p1[j], scalar_mont, qi, mred_params)


# NegVec returns p2 = -p1 mod qi.
def _mm_neg_vec(p1: list[u64xN], p2: list[u64xN], qi: u64xN):
	for j in range(len(p1)): p2[j] = qi - p1[j]


# AddVec returns p3 = p1 + p2 mod qi.
def _mm_add_vec(p1: list[u64xN], p2: list[u64xN], p3: list[u64xN], qi: u64xN):
    for j in range(len(p1)):
        p3[j] = _mm_cred(p1[j] + p2[j], qi)


# sub_vec returns p3 = p1 - p2 mod qi.
def _mm_sub_vec(p1: list[u64xN], p2: list[u64xN], p3: list[u64xN], qi: u64xN):
    for j in range(len(p1)):
        p3[j] = _mm_cred((p1[j] + qi) - p2[j], qi)


# mask_vec returns p2 = (p1>>w) & mask
def _mm_mask_vec(p1: list[u64xN], p2: list[u64xN], w: u64xN, mask: u64xN):
    for j in range(len(p1)):
        p2[j] = (p1[j] >> w) & mask


@llvm
def _mm_mask_half(u: u64xN) -> u64xN:
    %0 = shufflevector <4 x i64> zeroinitializer, <4 x i64> %u, <4 x i32> <i32 0, i32 0, i32 4, i32 4>
    ret <4 x i64> %0


@llvm
def _mm_mask_alternate(u: u64xN) -> u64xN:
    %0 = shufflevector <4 x i64> zeroinitializer, <4 x i64> %u, <4 x i32> <i32 0, i32 4, i32 0, i32 4>
    ret <4 x i64> %0


def _mm_ntt_lazy_x4(
        coeffs_in: list[u64xN],
        coeffs_out: list[u64xN],
        n: int,
        ntt_psi: list[u64],
        q: u64xN,
        q_inv: u64xN):
    assert SIMD_LANE_SIZE == 4
    
    # Copy the result of the first round of butterflies on p2 with approximate reduction
    two_q = q << u64xN(u64(1))
    four_q = q << u64xN(u64(2))
    two_q_masked_half = _mm_mask_half(two_q)
    two_q_masked_alternate = _mm_mask_alternate(two_q)
    t = n >> 1
    _mm_ntt_psi = u64xN(ntt_psi[1])

    t_mm = t >> 2
    for jx in range(t_mm):
        V = _mm_mred_constant(coeffs_in[jx + t_mm], _mm_ntt_psi, q, q_inv)
        coeffs_out[jx], coeffs_out[jx + t_mm] = coeffs_in[jx] + V, coeffs_in[jx] + two_q - V
    
	# Continue the rest of the second to the n-1 butterflies on p2 with approximate reduction
    m = 2
    while m < n:
        reduce = (m.bitlen() & 1) == 1
        t >>= 1
        t_mm = t >> 2

        if t >= 8:
            for i in range(m):
                j1 = (i * t_mm) << 1
                _mm_F = u64xN(ntt_psi[m + i])

                if reduce:
                    for jx in range(j1, j1 + t_mm):
                        coeffs_out[jx], coeffs_out[jx + t_mm] = _mm_butterfly(coeffs_out[jx], coeffs_out[jx + t_mm], _mm_F, two_q, four_q, q, q_inv)
                else:
                    for jx in range(j1, j1 + t_mm):
                        V = _mm_mred_constant(coeffs_out[jx + t_mm], _mm_F, q, q_inv)
                        coeffs_out[jx], coeffs_out[jx + t_mm] = coeffs_out[jx] + V, coeffs_out[jx] + two_q - V
        elif t == 4:
            if reduce:
                j1 = 0
                for i in range(m, m << 1, 2):
                    _mm_ntt_psi = u64xN(ntt_psi[i])
                    coeffs_out[j1], coeffs_out[j1 + 1] = _mm_butterfly(coeffs_out[j1], coeffs_out[j1 + 1], _mm_ntt_psi, two_q, four_q, q, q_inv)

                    _mm_ntt_psi = u64xN(ntt_psi[i + 1])
                    coeffs_out[j1 + 2], coeffs_out[j1 + 3] = _mm_butterfly(coeffs_out[j1 + 2], coeffs_out[j1 + 3], _mm_ntt_psi, two_q, four_q, q, q_inv)

                    j1 += 4
            else:
                j1 = 0
                for i in range(m, m << 1, 2):
                    _mm_ntt_psi = u64xN(ntt_psi[i])
                    V = _mm_mred_constant(coeffs_out[j1 + 1], _mm_ntt_psi, q, q_inv)
                    coeffs_out[j1], coeffs_out[j1 + 1] = coeffs_out[j1] + V, coeffs_out[j1] + two_q - V

                    _mm_ntt_psi = u64xN(ntt_psi[i + 1])
                    V = _mm_mred_constant(coeffs_out[j1 + 3], _mm_ntt_psi, q, q_inv)
                    coeffs_out[j1 + 2], coeffs_out[j1 + 3] = coeffs_out[j1 + 2] + V, coeffs_out[j1 + 2] + two_q - V

                    j1 += 4
        elif t == 2:
            if reduce:
                j1 = 0
                for i in range(m, m << 1, 4):
                    coeffs_out[j1] = _mm_butterfly_inplace_x2(coeffs_out[j1], u64xN(ntt_psi[i]), two_q_masked_half, four_q, q, q_inv)
                    coeffs_out[j1 + 1] = _mm_butterfly_inplace_x2(coeffs_out[j1 + 1], u64xN(ntt_psi[i + 1]), two_q_masked_half, four_q, q, q_inv)
                    coeffs_out[j1 + 2] = _mm_butterfly_inplace_x2(coeffs_out[j1 + 2], u64xN(ntt_psi[i + 2]), two_q_masked_half, four_q, q, q_inv)
                    coeffs_out[j1 + 3] = _mm_butterfly_inplace_x2(coeffs_out[j1 + 3], u64xN(ntt_psi[i + 3]), two_q_masked_half, four_q, q, q_inv)

                    j1 += 4
            else:
                j1 = 0
                for i in range(m, m << 1, 4):
                    coeffs_out[j1] = _mm_ntt_intrinsic_inplace(coeffs_out[j1], u64xN(ntt_psi[i]), two_q_masked_half, q, q_inv)
                    coeffs_out[j1 + 1] = _mm_ntt_intrinsic_inplace(coeffs_out[j1 + 1], u64xN(ntt_psi[i + 1]), two_q_masked_half, q, q_inv)
                    coeffs_out[j1 + 2] = _mm_ntt_intrinsic_inplace(coeffs_out[j1 + 2], u64xN(ntt_psi[i + 2]), two_q_masked_half, q, q_inv)
                    coeffs_out[j1 + 3] = _mm_ntt_intrinsic_inplace(coeffs_out[j1 + 3], u64xN(ntt_psi[i + 3]), two_q_masked_half, q, q_inv)

                    j1 += 4
        else:
            j1 = 0
            for i in range(m, m << 1, 8):
                _mm_ntt_psi = u64xN([ntt_psi[i], ntt_psi[i], ntt_psi[i + 1], ntt_psi[i + 1]])
                coeffs_out[j1] = _mm_butterfly_inplace_x1(coeffs_out[j1], _mm_ntt_psi, two_q_masked_alternate, four_q, q, q_inv)
                _mm_ntt_psi = u64xN([ntt_psi[i + 2], ntt_psi[i + 2], ntt_psi[i + 3], ntt_psi[i + 3]])
                coeffs_out[j1 + 1] = _mm_butterfly_inplace_x1(coeffs_out[j1 + 1], _mm_ntt_psi, two_q_masked_alternate, four_q, q, q_inv)
                _mm_ntt_psi = u64xN([ntt_psi[i + 4], ntt_psi[i + 4], ntt_psi[i + 5], ntt_psi[i + 5]])
                coeffs_out[j1 + 2] = _mm_butterfly_inplace_x1(coeffs_out[j1 + 2], _mm_ntt_psi, two_q_masked_alternate, four_q, q, q_inv)
                _mm_ntt_psi = u64xN([ntt_psi[i + 6], ntt_psi[i + 6], ntt_psi[i + 7], ntt_psi[i + 7]])
                coeffs_out[j1 + 3] = _mm_butterfly_inplace_x1(coeffs_out[j1 + 3], _mm_ntt_psi, two_q_masked_alternate, four_q, q, q_inv)

                j1 += 4

        m <<= 1


def _mm_invbutterfly_inplace_0_x4(coeffs: u64xN, ntt_psi_inv_0: u64, ntt_psi_inv_1: u64, two_q: u64, four_q: u64, q: u64, q_inv: u64) -> u64xN:
    coeffs_in = coeffs.scatter()
    c0, c1 = invbutterfly(coeffs_in[0], coeffs_in[1], ntt_psi_inv_0, two_q, four_q, q, q_inv)
    c2, c3 = invbutterfly(coeffs_in[2], coeffs_in[3], ntt_psi_inv_1, two_q, four_q, q, q_inv)
    return u64xN([c0, c1, c2, c3])


def _mm_invbutterfly_inplace_1_x4(coeffs: u64xN, ntt_psi_inv: u64, two_q: u64, four_q: u64, q: u64, q_inv: u64) -> u64xN:
    coeffs_out = coeffs.scatter()
    coeffs_out[0], coeffs_out[2] = invbutterfly(coeffs_out[0], coeffs_out[2], ntt_psi_inv, two_q, four_q, q, q_inv)
    coeffs_out[1], coeffs_out[3] = invbutterfly(coeffs_out[1], coeffs_out[3], ntt_psi_inv, two_q, four_q, q, q_inv)
    return u64xN(coeffs_out)


def _mm_inv_ntt_core_x4(coeffs_in: List[u64xN], coeffs_out: List[u64xN], n: int, ntt_psi_inv: List[u64], q: u64, q_inv: u64, _mm_q: u64xN, _mm_q_inv: u64xN):
    assert SIMD_LANE_SIZE == 4

    # Copy the result of the first round of butterflies on p2 with approximate reduction
    t = 1
    h = n >> 1
    two_q = q << 1
    four_q = q << 2
    _mm_two_q = u64xN(two_q)
    _mm_four_q = u64xN(four_q)

    j = 0
    for i in range(h, h << 1, 8):
        coeffs_out[j] = _mm_invbutterfly_inplace_0_x4(coeffs_in[j], ntt_psi_inv[i], ntt_psi_inv[i + 1], two_q, four_q, q, q_inv)
        coeffs_out[j + 1] = _mm_invbutterfly_inplace_0_x4(coeffs_in[j + 1], ntt_psi_inv[i + 2], ntt_psi_inv[i + 3], two_q, four_q, q, q_inv)
        coeffs_out[j + 2] = _mm_invbutterfly_inplace_0_x4(coeffs_in[j + 2], ntt_psi_inv[i + 4], ntt_psi_inv[i + 5], two_q, four_q, q, q_inv)
        coeffs_out[j + 3] = _mm_invbutterfly_inplace_0_x4(coeffs_in[j + 3], ntt_psi_inv[i + 6], ntt_psi_inv[i + 7], two_q, four_q, q, q_inv)
        j += 4

    # Continue the rest of the second to the n-1 butterflies on p2 with approximate reduction
    t <<= 1
    m = n >> 1
    
    while m > 1:
        _mm_t = t >> 2
        h = m >> 1

        if t >= 8:
            i, j1, j2 = 0, 0, t
            
            while i < h:
                _mm_j1 = j1 >> 2
                _mm_j2 = j2 >> 2
                _mm_f = u64xN(ntt_psi_inv[h + i])
                jx, jy = _mm_j1, _mm_j1 + _mm_t

                while jx < _mm_j2:
                    coeffs_out[jx], coeffs_out[jy] = _mm_invbutterfly(
                        coeffs_out[jx], coeffs_out[jy], _mm_f, _mm_two_q, _mm_four_q, _mm_q, _mm_q_inv)
                    jx, jy = jx + 1, jy + 1
                
                i, j1, j2 = i + 1, j1 + 2 * t, j2 + 2 * t

        elif t == 4:
            j1 = 0
            for i in range(h, h << 1, 2):
                _mm_ntt_psi = u64xN(ntt_psi_inv[i])
                coeffs_out[j1], coeffs_out[j1 + 1] = _mm_invbutterfly(
                    coeffs_out[j1], coeffs_out[j1 + 1], _mm_ntt_psi, _mm_two_q, _mm_four_q, _mm_q, _mm_q_inv)
                _mm_ntt_psi = u64xN(ntt_psi_inv[i + 1])
                coeffs_out[j1 + 2], coeffs_out[j1 + 3] = _mm_invbutterfly(
                    coeffs_out[j1 + 2], coeffs_out[j1 + 3], _mm_ntt_psi, _mm_two_q, _mm_four_q, _mm_q, _mm_q_inv)
                j1 += 4
        else:
            j1 = 0
            for i in range(h, h << 1, 4):
                coeffs_out[j1] = _mm_invbutterfly_inplace_1_x4(coeffs_out[j1], ntt_psi_inv[i], two_q, four_q, q, q_inv)
                coeffs_out[j1 + 1] = _mm_invbutterfly_inplace_1_x4(coeffs_out[j1 + 1], ntt_psi_inv[i + 1], two_q, four_q, q, q_inv)
                coeffs_out[j1 + 2] = _mm_invbutterfly_inplace_1_x4(coeffs_out[j1 + 2], ntt_psi_inv[i + 2], two_q, four_q, q, q_inv)
                coeffs_out[j1 + 3] = _mm_invbutterfly_inplace_1_x4(coeffs_out[j1 + 3], ntt_psi_inv[i + 3], two_q, four_q, q, q_inv)
                j1 += 4

        t <<= 1
        m >>= 1


# NTT computes the NTT on the input coefficients using the input parameters.
def _mm_ntt(coeffs_in: list[u64xN], coeffs_out: list[u64xN], n: int, ntt_psi: list[u64], q: u64xN, mred_params: u64xN, bred_params: u64xN):
    _mm_ntt_lazy_x4( coeffs_in, coeffs_out, n, ntt_psi, q, mred_params)
    _mm_reduce_vec(coeffs_out, coeffs_out, q, bred_params)


# InvNTT computes the InvNTT transformation on the input coefficients using the input parameters.
def _mm_inv_ntt(coeffs_in: list[u64xN], coeffs_out: list[u64xN], n: int, ntt_psi_inv: list[u64], ntt_n_inv: u64xN, q: u64xN, q_inv: u64xN, q_u64: u64, q_inv_u64: u64):
    _mm_inv_ntt_core_x4(coeffs_in, coeffs_out, n, ntt_psi_inv, q_u64, q_inv_u64, q, q_inv)
    _mm_mul_scalar_montgomery_vec(coeffs_out, coeffs_out, ntt_n_inv, q, q_inv)


# InvNTTLazy computes the InvNTT transformation on the input coefficients using the input parameters with output values in the range [0, 2q-1].
def _mm_inv_ntt_lazy(coeffs_in: list[u64xN], coeffs_out: list[u64xN], n: int, ntt_psi_inv: list[u64], ntt_n_inv: u64xN, q: u64xN, q_inv: u64xN, q_u64: u64, q_inv_u64: u64):
    _mm_inv_ntt_core_x4(coeffs_in, coeffs_out, n, ntt_psi_inv, q_u64, q_inv_u64, q, q_inv)
    _mm_mul_scalar_montgomery_constant_vec(coeffs_out, coeffs_out, ntt_n_inv, q, q_inv)


# Poly is the structure that contains the coefficients of a polynomial.
class Poly:
    coeffs: list[list[u64]]
    # Temp vectorized coeffs
    _mm_coeffs: list[list[u64xN]]
    is_ntt: bool
    is_mform: bool

    def temp_vectorize(self):
        self._mm_coeffs = self.coeffs.vectorize(SIMD_LANE_SIZE)
    
    def temp_scatter(self):
        self.coeffs = self._mm_coeffs.scatter_inplace()
    
    def __bool__(self):
        return len(self.coeffs) > 0
    
    def __eq__(self, other: Poly) -> bool:
        return self.coeffs == other.coeffs
    
    def __ne__(self, other: Poly) -> bool:
         return not self == other
        
    def __repr__(self) -> str:
        return f"""\tPolynomial:
                \tCoeffs[0][:3]: {self.coeffs[0][:3]}
        """
    
    def __copy__(self) -> Poly:
        return self.copy()
    
    def __pickle__(self, jar: Jar, pasteurized: bool):
        pickle(self.coeffs, jar, pasteurized)
        if not pasteurized: jar += self.coeffs.pickle_size()
        pickle(self.is_ntt, jar, pasteurized)
        if not pasteurized: jar += self.is_ntt.pickle_size()
        pickle(self.is_mform, jar, pasteurized)
    
    def __unpickle__(jar: Jar, pasteurized: bool) -> Poly:
        coeffs = unpickle(jar, pasteurized, list[list[u64]])
        if not pasteurized: jar += coeffs.pickle_size()
        is_ntt = unpickle(jar, pasteurized, bool)
        if not pasteurized: jar += is_ntt.pickle_size()
        is_mform = unpickle(jar, pasteurized, bool)
        pol = Poly(
            coeffs=coeffs,
            is_ntt=is_ntt,
            is_mform=is_mform)
        
        return pol
    
    def pickle_size(self) -> int:
        return self.coeffs.pickle_size() + \
            self.is_ntt.pickle_size() + \
            self.is_mform.pickle_size()

    def _mm_set(self, other: Poly):
        self._mm_coeffs = other._mm_coeffs
        self.is_ntt = other.is_ntt
        self.is_mform = other.is_mform
    
    def copy_values(self, other: Poly):
        self.coeffs = [copy(row) for row in other.coeffs]
        self._mm_coeffs = [copy(row) for row in other._mm_coeffs]
    
    def copy(self) -> Poly:
        new_poly = Poly()
        new_poly.copy_values(self)
        new_poly.is_ntt = self.is_ntt
        new_poly.is_mform = self.is_mform
        return new_poly

    # n returns the number of coefficients of the polynomial, which equals the degree of the Ring cyclotomic polynomial.
    def n(self):
        return len(self.coeffs[0])

    # level returns the current number of moduli minus 1.
    def level(self):
        return len(self.coeffs) - 1
    
    # resize resizes the level of the target polynomial to the provided level.
    # If the provided level is larger than the current level, then allocates zero
    # coefficients, otherwise dereferences the coefficients above the provided level.
    def resize(self, level: int):
        n = self.n()
        if self.level() > level:
            self.coeffs = self.coeffs[:level + 1]
        elif level > self.level():
            for _ in range(level - self.level()):
                self.coeffs.append(zeros_vec(n, TP=u64))
    
    def randomize(self, upper_bound: int):
        self.coeffs = self.coeffs.rand(u64(upper_bound))


# new_poly creates a new polynomial with n coefficients set to zero and level+1 moduli.
def new_poly(n, level):
    coeffs = zeros(level + 1, n, TP=u64)
    _mm_coeffs = coeffs.vectorize(SIMD_LANE_SIZE)
    return Poly(coeffs=coeffs, _mm_coeffs=_mm_coeffs)


# NumberTheoreticTransformerStandard computes the standard nega-cyclic NTT in the ring Z[X]/(X^n+1).
class NumberTheoreticTransformerStandard:
    name: str

    def __init__(self):
        self.name = "standard"
    
    # forward_lvl writes the forward NTT in Z[X]/(X^n+1) of p1 on p2.
    # Only computes the NTT for the first level+1 moduli.
    def _mm_forward_lvl(r, level: int, p1: Poly, p2: Poly):
        for x in range(level + 1):
            _mm_ntt(p1._mm_coeffs[x], p2._mm_coeffs[x], r.n, r.ntt_psi[x], r._mm_modulus[x], r._mm_mred_params[x], r._mm_bred_params[x][0])

    # backward_lvl writes the backward NTT in Z[X]/(X^n+1) on p2.
    # Only computes the NTT for the first level+1 moduli.
    def _mm_backward_lvl(r, level, p1, p2):
        for x in range(level + 1):
            _mm_inv_ntt(p1._mm_coeffs[x], p2._mm_coeffs[x], r.n, r.ntt_psi_inv[x], r._mm_ntt_n_inv[x], r._mm_modulus[x], r._mm_mred_params[x], r.modulus[x], r.mred_params[x])
    
    # BackwardLazyLvl writes the backward NTT in Z[X]/(X^n+1) on p2.
    # Only computes the NTT for the first level+1 moduli and returns values in the range [0, 2q-1].
    def _mm_backward_lazy_lvl(r, level: int, p1: Poly, p2: Poly):
        for x in range(level + 1):
            _mm_inv_ntt_lazy(p1._mm_coeffs[x], p2._mm_coeffs[x], r.n, r.ntt_psi_inv[x], r._mm_ntt_n_inv[x], r._mm_modulus[x], r._mm_mred_params[x], r.modulus[x], r.mred_params[x])
    
    # forward_lazy_lvl writes the forward NTT in Z[X]/(X^n+1) of p1 on p2.
    # Only computes the NTT for the first level+1 moduli and returns values in the range [0, 2q-1].
    def _mm_forward_lazy_lvl(r, level, p1, p2):
        for x in range(level + 1):
            _mm_ntt_lazy_x4(p1._mm_coeffs[x], p2._mm_coeffs[x], r.n, r.ntt_psi[x], r._mm_modulus[x], r._mm_mred_params[x])
    
    # ForwardVec writes the forward NTT in Z[X]/(X^n+1) of the i-th level of p1 on the i-th level of p2.
    def _mm_forward_vec(r, level: int, p1: list[u64xN], p2: list[u64xN]):
        _mm_ntt(p1, p2, r.n, r.ntt_psi[level], r._mm_modulus[level], r._mm_mred_params[level], r._mm_bred_params[level][0])

    # ForwardLazyVec writes the forward NTT in Z[X]/(X^n+1) of the i-th level of p1 on the i-th level of p2.
    # Returns values in the range [0, 2q-1].
    def _mm_forward_lazy_vec(r, level: int, p1: list[u64xN], p2: list[u64xN]):
        _mm_ntt_lazy_x4(p1, p2, r.n, r.ntt_psi[level], r._mm_modulus[level], r._mm_mred_params[level])
    
    # BackwardLazyVec writes the backward NTT in Z[X]/(X^N+1) of the i-th level of p1 on the i-th level of p2.
    # Returns values in the range [0, 2q-1].
    def _mm_backward_lazy_vec(r, level: int, p1: list[u64xN], p2: list[u64xN]):
        _mm_inv_ntt_lazy(p1, p2, r.n, r.ntt_psi_inv[level], r._mm_ntt_n_inv[level], r._mm_modulus[level], r._mm_mred_params[level], r.modulus[level], r.mred_params[level])


# NumberTheoreticTransformerConjugateInvariant computes the NTT in the ring Z[X+X^-1]/(X^2N+1).
# Z[X+X^-1]/(X^2N+1) is a closed sub-ring of Z[X]/(X^2N+1). Note that the input polynomial only needs to self size n
# since the right half does not provide any additional information.
# See "Approximate Homomorphic Encryption over the Conjugate-invariant Ring", https:#eprint.iacr.org/2018/952.
# The implemented approach is more efficient than the one proposed in the referenced work.
# It avoids the linear map Z[X + X^-1]/(X^2N + 1) <-> Z[X]/(X^n - 1) by instead directly computing the left
# half of the NTT of Z[X + X^-1]/(X^2N + 1) since the right half provides no additional information, which
# allows to (re)use nega-cyclic NTT.
class NumberTheoreticTransformerConjugateInvariant:
    name: str

    def __init__(self):
        self.name = "conjugate_invariant"


class Ring:
    # Polynomial nb.Coefficients
    n: int
    _mm_n: int
    # moduli
    modulus: list[u64]
    _mm_modulus: list[u64xN]
    # 2^bit_length(Qi) - 1
    mask: list[u64]
    # Indicates whether NTT can self used with the current ring.
    allows_ntt: bool
    # Product of the moduli for each level
    modulus_at_level: list[big_uint]
    # Fast reduction parameters
    bred_params: list[list[u64]]
    _mm_bred_params: list[Tuple[u64xN, u64xN]]
    mred_params: list[u64]
    _mm_mred_params: list[u64xN]
    rescale_params: list[list[u64]]
    _mm_rescale_params: list[list[u64xN]]
    # NTT Parameters
    nth_root: u64
    psi_mont: list[u64]  #2N-th primitive root in Montgomery form
    psi_inv_mont: list[u64]  #2N-th inverse primitive root in Montgomery form
    ntt_psi: list[list[u64]]  #powers of the inverse of the 2N-th primitive root in Montgomery form (in bit-reversed order)
    ntt_psi_inv: list[list[u64]]  #powers of the inverse of the 2N-th primitive root in Montgomery form (in bit-reversed order)
    ntt_n_inv: list[u64]  #[n^-1] mod Qi in Montgomery form
    _mm_ntt_n_inv: list[u64xN]

    ntt_type: str

    def __bool__(self):
        return bool(self.n)
    
    def __repr__(self):
        return f"""\tRing:
                \tN: {self.n}
                \tModuli[:10]: {self.modulus[:10]}
                \mask[:10]: {self.mask[:10]}
                \modulus_at_level[:10]: {self.modulus_at_level[:10]}
        """
    
    def set_parameters(self, n, modulus):
	    # Checks if n is a power of 2
        if (n < 16) or (n & (n - 1)) != 0 and n != 0:
            raise ValueError("invalid ring degree (must self a power of 2 >= 8)")

        if len(modulus) == 0:
            raise ValueError("invalid modulus (must self a non-empty []u64)")

        if not utils.all_distinct(modulus):
            raise ValueError("invalid modulus (moduli are not distinct)")

        self.allows_ntt = False
        self.n = n
        self._mm_n = n // SIMD_LANE_SIZE

        self.modulus = list[u64](len(modulus))
        self._mm_modulus = list[u64xN](len(modulus))
        self.mask = list[u64](len(modulus))

        for qi in modulus:
            self.modulus.append(qi)
            self._mm_modulus.append(u64xN(qi))
            self.mask.append((u64(1) << qi.bitlen()) - u64(1))

        # Computes bigQ for all levels
        self.modulus_at_level = list[big_uint](len(modulus))
        self.modulus_at_level.append(self.modulus[0].ext_to_bigint())

        num_bits_total = 1
        for m in self.modulus:
            num_bits_total += int(m.bitlen())

        assert num_bits_total <= BIG_INT_LEN, (
            f"moduli product too large for the bigint in use."
            f" Increase the bigint size. Current bigint size: {BIG_INT_LEN}."
            f" Required size: {num_bits_total}")

        for i in range(1, len(self.modulus)):
            self.modulus_at_level.append(self.modulus_at_level[i - 1] * self.modulus[i].ext_to_bigint())
        
        # Computes the fast reduction parameters
        self.bred_params = list[list[u64]](len(modulus))
        self.mred_params = list[u64](len(modulus))

        for i, qi in enumerate(self.modulus):
            # Computes the fast modular reduction parameters for the Ring
            self.bred_params.append(bred_params(qi))
            self._mm_bred_params.append(
                (u64xN(self.bred_params[i][0]), u64xN(self.bred_params[i][1])))

            # If qi is not a power of 2, we can compute the MRedParams (otherwise, it
            # would return an error as there is no valid Montgomery form mod a power of 2)
            if (qi & (qi - u64(1))) != u64(0) and qi != u64(0):
                self.mred_params.append(mred_params(qi))
                self._mm_mred_params.append(u64xN(self.mred_params[i]))
            else:
                self.mred_params.append(u64(0))
                self._mm_mred_params.append(u64xN(u64(0)))
        
    def set_number_theoretic_transformer(self, ntt):
        self.ntt_type = ntt
    
    # type returns the type of the ring which might self either `StandardRing` or `ConjugateInvariantRing`.
    def type(self) -> int:
        if self.ntt_type == NumberTheoreticTransformerStandard().name:
            return StandardRing
        elif self.ntt_type ==  NumberTheoreticTransformerConjugateInvariant().name:
            return ConjugateInvariantRing
        else: raise ValueError("invalid NumberTheoreticTransformer type")

    # gen_ntt_params checks that n has been correctly initialized, and checks that each modulus is a prime congruent to 1 mod 2N (i.e. NTT-friendly).
    # Then, it computes the variables required for the NTT. The purpose of ValidateParameters is to validate that the moduli allow the NTT, and to compute the
    # NTT parameters.
    def gen_ntt_params(self, nth_root):
        if self.n == 0 or not self.modulus:
            raise ValueError("invalid r parameters (missing)")

        if self.n == 0 or not self.modulus or nth_root < u64(1):
            raise ValueError("error : invalid r parameters (missing)")

        # Checks if each qi is prime and equal to 1 mod nth_root
        for i, qi in enumerate(self.modulus):
            if not utils.is_prime(qi):
                raise ValueError(f"invalid modulus (modulus[{i}] is not prime)")

            if qi & (nth_root - u64(1)) != u64(1):
                self.allows_ntt = False
                raise ValueError("invalid modulus (modulus[{i}] != 1 mod nth_root)")

        self.nth_root = nth_root
        self.rescale_params = [[] for _ in range(len(self.modulus) - 1)]

        for j in range(len(self.modulus) - 1, 0, -1):
            self.rescale_params[j - 1] = list[u64](j)

            for i in range(j):
                self.rescale_params[j - 1].append(
                    mform(
                        self.modulus[i] - mod_exp_u64(self.modulus[j], self.modulus[i] - u64(2), self.modulus[i]),
                        self.modulus[i], self.bred_params[i]))
        
        self._mm_rescale_params = [[u64xN(e) for e in row] for row in self.rescale_params]
        
        self.psi_mont = list[u64](len(self.modulus))
        self.psi_inv_mont = list[u64](len(self.modulus))
        self.ntt_psi = [[] for _ in range(len(self.modulus))]
        self.ntt_psi_inv = [[] for _ in range(len(self.modulus))]
        self.ntt_n_inv = list[u64](len(self.modulus))

        log_nth_root = int((nth_root >> u64(1)).bitlen()) - 1

        for i, qi in enumerate(self.modulus):
            # 1.1 Computes n^(-1) mod q in Montgomery form
            self.ntt_n_inv.append(mform(mod_exp_u64(nth_root >> u64(1), qi - u64(2), qi), qi, self.bred_params[i]))
            self._mm_ntt_n_inv.append(u64xN(self.ntt_n_inv[i]))

            # 1.2 Computes Psi and PsiInv in Montgomery form
            self.ntt_psi[i] = zeros_vec(int(nth_root >> u64(1)), TP=u64)
            self.ntt_psi_inv[i] = zeros_vec(int(nth_root >> u64(1)), TP=u64)

            # Finds a 2N-th primitive Root
            g = primitive_root(qi)

            power = (qi - u64(1)) // nth_root
            powerInv = (qi - u64(1)) - power

            # Computes Psi and PsiInv in Montgomery form
            psi_mont = mform(mod_exp_u64(g, power, qi), qi, self.bred_params[i])
            psi_inv_mont = mform(mod_exp_u64(g, powerInv, qi), qi, self.bred_params[i])

            self.psi_mont.append(psi_mont)
            self.psi_inv_mont.append(psi_inv_mont)

            self.ntt_psi[i][0] = mform(u64(1), qi, self.bred_params[i])
            self.ntt_psi_inv[i][0] = mform(u64(1), qi, self.bred_params[i])

            # Computes ntt_psi[j] = ntt_psi[j-1]*Psi and ntt_psi_inv[j] = ntt_psi_inv[j-1]*PsiInv
            for j in range(1, int(nth_root >> u64(1))):
                index_reverse_prev = (j - 1).bit_reverse_subset(log_nth_root)
                index_reverse_next = j.bit_reverse_subset(log_nth_root)

                self.ntt_psi[i][index_reverse_next] = mred(
                    self.ntt_psi[i][index_reverse_prev], psi_mont, qi, self.mred_params[i])
                self.ntt_psi_inv[i][index_reverse_next] = mred(
                    self.ntt_psi_inv[i][index_reverse_prev], psi_inv_mont, qi, self.mred_params[i])

        self.allows_ntt = True

    # new_poly creates a new polynomial with all coefficients set to 0.
    def new_poly(self):
        return new_poly(self.n, len(self.modulus) - 1)
    
    # new_poly_lvl creates a new polynomial with all coefficients set to 0.
    def new_poly_lvl(self, level):
        return new_poly(self.n, level)
    
    def check_lvls(p1: Poly, p2: Poly) -> int:
        lvl1, lvl2 = p1.level(), p2.level()

        if lvl1 != lvl2:
            print(f"WARNING: Incompatible levels of polynomials -- {lvl1} != {lvl2}")
        
        return min(lvl1, lvl2)
    
    # PermuteNTT applies the Galois transform on a polynomial in the NTT domain.
    # It maps the coefficients x^i to x^(gen*i)
    # It must be noted that the result cannot be in-place.
    def permute_ntt(self, pol_in: Poly, gen: u64, pol_out: Poly):
        self.permute_ntt_lvl(min(pol_in.level(), pol_out.level()), pol_in, gen, pol_out)
    
    # PermuteNTTLvl applies the Galois transform on a polynomial in the NTT domain, up to a given level.
    # It maps the coefficients x^i to x^(gen*i)
    # It must be noted that the result cannot be in-place.
    def permute_ntt_lvl(self, level: int, pol_in: Poly, gen: u64, pol_out: Poly):
        self.permute_ntt_with_index_lvl(level, pol_in, self.permute_ntt_index(gen), pol_out)

    # permute_ntt_index computes the index table for PermuteNTT.
    def permute_ntt_index(self, gal_el: u64) -> list[u64]:
        log_nth_root = int(self.nth_root.bitlen() - u64(2))
        mask = self.nth_root - u64(1)
        index = list[u64](self.n)

        for i in range(self.n):
            tmp_1 = u64(2) * u64(i.bit_reverse_subset(log_nth_root)) + u64(1)
            tmp_2 = ((gal_el * tmp_1 & mask) - u64(1)) >> u64(1)
            index.append(u64(tmp_2.bit_reverse_subset(log_nth_root)))

        return index
    
    # permute_ntt_with_index_lvl applies the Galois transform on a polynomial in the NTT domain, up to a given level.
    # It maps the coefficients x^i to x^(gen*i) using the PermuteNTTIndex table.
    # It must be noted that the result cannot be in-place.
    def permute_ntt_with_index_lvl(self, level: int, pol_in: Poly, index: list[u64], pol_out: Poly):
        for j in range(0, self.n, 8):
            for i in range(level + 1):
                y = pol_in.coeffs[i]
                for k in range(8): pol_out.coeffs[i][j + k] = y[int(index[j + k])]
    
    # PermuteLvl applies the Galois transform on a polynomial outside of the NTT domain.
    # It maps the coefficients x^i to x^(gen*i).
    # It must be noted that the result cannot be in-place.
    def permute_lvl(self, level: int, pol_in: Poly, gen: u64, pol_out: Poly):
        mask = u64(self.n - 1)
        log_n = mask.bitlen()

        for i in range(self.n):
            index_raw = u64(i) * gen
            index = int(index_raw & mask)
            tmp = (index_raw >> log_n) & u64(1)

            for j, qi in enumerate(self.modulus[:level + 1]):
                pol_out.coeffs[j][index] = pol_in.coeffs[j][i] * (tmp ^ u64(1)) | (qi - pol_in.coeffs[j][i]) * tmp
    
    def min_level_binary(self, p1, p2):
        return min(min(len(self.modulus) - 1, p1.level()), p2.level())

    def min_level_ternary(self, p1, p2, p3):
        return min(min(len(self.modulus) - 1, p1.level()), min(p2.level(), p3.level()))
    
    # ntt_lvl computes the NTT of p1 and returns the result on p2.
    # The value level defines the number of moduli of the input polynomials.
    def _mm_ntt_lvl(self, level: int, p1: Poly, p2: Poly):
        if self.ntt_type == NumberTheoreticTransformerStandard().name:
            NumberTheoreticTransformerStandard._mm_forward_lvl(self, level, p1, p2)
        else: raise NotImplementedError()
    
    # ntt_single computes the NTT of p1 and returns the result on p2.
    # The level-th moduli of the ring NTT params are used.
    def _mm_ntt_single(self, level: int, p1: list[u64xN], p2: list[u64xN]):
        if self.ntt_type == NumberTheoreticTransformerStandard().name:
            NumberTheoreticTransformerStandard._mm_forward_vec(self, level, p1, p2)
        else: raise NotImplementedError()
    
    # NTTSingleLazy computes the NTT of p1 and returns the result on p2.
    # The level-th moduli of the ring NTT params are used.
    # Output values are in the range [0, 2q-1]
    def _mm_ntt_single_lazy(self, level: int, p1: list[u64xN], p2: list[u64xN]):
        if self.ntt_type == NumberTheoreticTransformerStandard().name:
            NumberTheoreticTransformerStandard._mm_forward_lazy_vec(self, level, p1, p2)
        else: raise NotImplementedError()
    
    # ntt_lazy_lvl computes the NTT of p1 and returns the result on p2.
    # The value level defines the number of moduli of the input polynomials.
    # Output values are in the range [0, 2q-1]
    def _mm_ntt_lazy_lvl(self, level: int, p1: Poly, p2: Poly):
        if self.ntt_type == NumberTheoreticTransformerStandard().name:
            NumberTheoreticTransformerStandard._mm_forward_lazy_lvl(self, level, p1, p2)
        else: raise NotImplementedError()
    
    # inv_ntt_lvl computes the inverse-NTT of p1 and returns the result on p2.
    # The value level defines the number of moduli of the input polynomials.
    def _mm_inv_ntt_lvl(self, level, p1, p2):
        if self.ntt_type == NumberTheoreticTransformerStandard().name:
            NumberTheoreticTransformerStandard._mm_backward_lvl(self, level, p1, p2)
        else: raise NotImplementedError()
    
    # InvNTTLazyLvl computes the inverse-NTT of p1 and returns the result on p2.
    # The value level defines the number of moduli of the input polynomials.
    # Output values are in the range [0, 2q-1]
    def _mm_inv_ntt_lazy_lvl(self, level: int, p1: Poly, p2: Poly):
        if self.ntt_type == NumberTheoreticTransformerStandard().name:
            NumberTheoreticTransformerStandard._mm_backward_lazy_lvl(self, level, p1, p2)
        else: raise NotImplementedError()

    # InvNTTSingleLazy computes the InvNTT of p1 and returns the result on p2.
    # The level-th moduli of the ring InvNTT params are used.
    # Output values are in the range [0, 2q-1]
    def _mm_inv_ntt_single_lazy(self, level: int, p1: list[u64xN], p2: list[u64xN]):
        if self.ntt_type == NumberTheoreticTransformerStandard().name:
            NumberTheoreticTransformerStandard._mm_backward_lazy_vec(self, level, p1, p2)
        else: raise NotImplementedError()

    # mform_lvl switches p1 to the Montgomery domain for the moduli from q_0 up to q_level and writes the result on p2.
    def _mm_mform_lvl(self, level, p1, p2):
        for i in range(level + 1):
            _mm_mform_vec(p1._mm_coeffs[i], p2._mm_coeffs[i], self._mm_modulus[i], self._mm_bred_params[i])

    # InvMForm switches back p1 from the Montgomery domain to the conventional domain and writes the result on p2.
    def _mm_inv_mform(self, p1: Poly, p2: Poly):
        self._mm_inv_mform_lvl(self.min_level_binary(p1, p2), p1, p2)
    
    # InvMFormLvl switches back p1 from the Montgomery domain to the conventional domain and writes the result on p2.
    def _mm_inv_mform_lvl(self, level: int, p1: Poly, p2: Poly):
        for i in range(level + 1):
            _mm_inv_mform_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], self._mm_modulus[i], self._mm_mred_params[i])
    
    # mul_coeffs_montgomery multiplies p1 by p2 coefficient-wise with a
    # Montgomery modular reduction and returns the result on p3.
    def _mm_mul_coeffs_montgomery(self, p1, p2, p3):
        self._mm_mul_coeffs_montgomery_lvl(self.min_level_ternary(p1, p2, p3), p1, p2, p3)
    
    # mul_coeffs_montgomery_lvl multiplies p1 by p2 coefficient-wise with a Montgomery
    # modular reduction for the moduli from q_0 up to q_level and returns the result on p3.
    def _mm_mul_coeffs_montgomery_lvl(self, level, p1, p2, p3):
        for i in range(level + 1):
            _mm_mul_coeffs_montgomery_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], p3._mm_coeffs[i], self._mm_modulus[i], self._mm_mred_params[i])
    
    # MulScalarBigint multiplies each coefficient of p1 by a big.Int scalar and writes the result on p2.
    def _mm_mul_scalar_bigint(self, p1, scalar, p2):
        self._mm_mul_scalar_bigint_lvl(self.min_level_binary(p1, p2), p1, scalar, p2)

    # mul_scalar_bigint_lvl multiplies each coefficient of p1 by a big.Int scalar
    # for the moduli from q_0 up to q_level and writes the result on p2.
    def _mm_mul_scalar_bigint_lvl(self, level, p1, scalar, p2):
        for i in range(level + 1):
            _mm_scalarQi = u64xN(biguint_mod(scalar, self.modulus[i].ext_to_bigint()).trunc_to(64))
            _mm_m = _mm_mform(_mm_bred_add(_mm_scalarQi, self._mm_modulus[i], self._mm_bred_params[i][0]), self._mm_modulus[i], self._mm_bred_params[i])
            _mm_mul_scalar_montgomery_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], _mm_m, self._mm_modulus[i], self._mm_mred_params[i])
    
    # MulcoeffsMontgomeryConstantLvl multiplies p1 by p2 coefficient-wise with a Montgomery
    # modular reduction for the moduli from q_0 up to q_level and returns the result on p3.
    def _mm_mul_coeffs_montgomery_constant_lvl(self, level: int, p1: Poly, p2: Poly, p3: Poly):
        for i in range(level + 1):
            _mm_mul_coeffs_montgomery_constant_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], p3._mm_coeffs[i],
                self._mm_modulus[i], self._mm_mred_params[i])
    
    # mul_scalar multiplies each coefficient of p1 by a scalar and writes the result on p2.
    def _mm_mul_scalar(self, p1, scalar, p2):
        self._mm_mul_scalar_lvl(self.min_level_binary(p1, p2), p1, scalar, p2)
    
    # mul_scalar_lvl multiplies each coefficient of p1 by a scalar for the moduli from q_0 up to q_level and writes the result on p2.
    def _mm_mul_scalar_lvl(self, level, p1, scalar, p2):
        for i in range(level + 1):
            _mm_m = u64xN(mform(bred_add(u64(scalar), self.modulus[i], self.bred_params[i][0]), self.modulus[i], self.bred_params[i]))
            _mm_mul_scalar_montgomery_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], _mm_m, self._mm_modulus[i], self._mm_mred_params[i])
    
    # MulcoeffsMontgomeryAndAddLvl multiplies p1 by p2 coefficient-wise with a Montgomery
    # modular reduction for the moduli from q_0 up to q_level and adds the result to p3.
    def _mm_mul_coeffs_montgomery_and_add_lvl(self, level: int, p1: Poly, p2: Poly, p3: Poly):
        for i in range(level + 1):
            _mm_mul_coeffs_montgomery_and_add_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], p3._mm_coeffs[i],
                self._mm_modulus[i], self._mm_mred_params[i])

    # mul_coeffs_montgomery_and_sub_lvl multiplies p1 by p2 coefficient-wise with
    # a Montgomery modular reduction and subtracts the result from p3.
    def _mm_mul_coeffs_montgomery_and_sub_lvl(self, level, p1, p2, p3):
        for i in range(level + 1):
            _mm_mul_coeffs_montgomery_and_sub_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], p3._mm_coeffs[i],
                self._mm_modulus[i], self._mm_mred_params[i])
    
    # MulcoeffsMontgomeryConstantAndAddNoModLvl multiplies p1 by p2 coefficient-wise with a constant-time Montgomery
    # modular reduction for the moduli from q_0 up to q_level and adds the result to p3 without modular reduction.
    # Return values in [0, 3q-1]
    def _mm_mul_coeffs_montgomery_constant_and_add_no_mod_lvl(self, level: int, p1: Poly, p2: Poly, p3: Poly):
        for i in range(level + 1):
            _mm_mul_coeffs_montgomery_constant_and_add_no_mod_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], p3._mm_coeffs[i],
                self._mm_modulus[i], self._mm_mred_params[i])
    
    # neg_lvl sets the coefficients of p1 to their additive inverse for
    # the moduli from q_0 up to q_level and writes the result on p2.
    def _mm_neg_lvl(self, level: int, p1: Poly, p2: Poly):
        for i in range(level + 1):
            _mm_neg_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], self._mm_modulus[i])
    
    # add_lvl adds p1 to p2 coefficient-wise for the moduli from
    # q_0 up to q_level and writes the result on p3.
    def _mm_add_lvl(self, level: int, p1: Poly, p2: Poly, p3: Poly):
        for i in range(level + 1):
            _mm_add_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], p3._mm_coeffs[i], self._mm_modulus[i])
    
    # sub_lvl subtracts p2 to p1 coefficient-wise and writes the result on p3.
    def _mm_sub_lvl(self, level: int, p1: Poly, p2: Poly, p3: Poly):
        for i in range(level + 1):
            _mm_sub_vec(
                p1._mm_coeffs[i].slice_reference(0, self._mm_n),
                p2._mm_coeffs[i], p3._mm_coeffs[i], self._mm_modulus[i])
    
    # reduce_lvl applies a modular reduction on the coefficients of p1
    # for the moduli from q_0 up to q_level and writes the result on p2.
    def _mm_reduce_lvl(self, level: int, p1: Poly, p2: Poly):
        for i in range(level + 1):
            _mm_reduce_vec(p1._mm_coeffs[i], p2._mm_coeffs[i], self._mm_modulus[i], self._mm_bred_params[i][0])
    
    # DivRoundByLastModulusLvl divides (rounded) the polynomial by its last modulus. The input must be in the NTT domain.
    # Output poly level must be equal or one less than input level.
    def _mm_div_round_by_last_modulus_lvl(self, level: int, p0: Poly, p1: Poly):
        # Center by (p-1)/2
        _mm_phalf = u64xN((self.modulus[level] - 1) >> 1)
        _mm_add_scalar_vec(p0._mm_coeffs[level], p0._mm_coeffs[level], _mm_phalf, self._mm_modulus[level])

        for i in range(level):
            _mm_qi = self._mm_modulus[i]
            _mm_add_scalar_no_mod_and_neg_two_qi_no_mod_vec(p0._mm_coeffs[i], p0._mm_coeffs[i], _mm_qi - _mm_bred_add(_mm_phalf, _mm_qi, self._mm_bred_params[i][0]), _mm_qi)
            _mm_add_vec_no_mod_and_mul_scalar_montgomery_vec(p0._mm_coeffs[level], p0._mm_coeffs[i], p1._mm_coeffs[i], self._mm_rescale_params[level - 1][i], _mm_qi, self._mm_mred_params[i])

    # DivRoundByLastModulusNTTLvl divides (rounded) the polynomial by its last modulus. The input must be in the NTT domain.
    # Output poly level must be equal or one less than input level.
    def _mm_div_round_by_last_modulus_ntt_lvl(self, level: int, p0: Poly, buff: Poly, p1: Poly):
        self._mm_inv_ntt_single_lazy(level, p0._mm_coeffs[level], buff._mm_coeffs[level])

        # Center by (p-1)/2
        p_half = (self.modulus[level] - 1) >> 1
        _mm_p_half = u64xN(p_half)
        _mm_add_scalar_vec(buff._mm_coeffs[level], buff._mm_coeffs[level], _mm_p_half, self._mm_modulus[level])

        for i in range(level):
            qi = self.modulus[i]
            _mm_scalar = u64xN(qi - bred_add(p_half, qi, self.bred_params[i][0]))
            _mm_add_scalar_no_mod_vec(buff._mm_coeffs[level], buff._mm_coeffs[i], _mm_scalar)
            self._mm_ntt_single_lazy(i, buff._mm_coeffs[i], buff._mm_coeffs[i])
            _mm_sub_vec_and_mul_scalar_montgomery_two_qi_vec(buff._mm_coeffs[i], p0._mm_coeffs[i], p1._mm_coeffs[i], self._mm_rescale_params[level - 1][i], self._mm_modulus[i], self._mm_mred_params[i])

    # DivRoundByLastModulusManyNTTLvl divides (rounded) sequentially nb_rescales times the polynomial by its last modulus. The input must be in the NTT domain.
    # Output poly level must be equal or nb_rescales less than input level.
    def _mm_div_round_by_last_modulus_many_ntt_lvl(self, level: int, nb_rescales: int, p0: Poly, buff: Poly, p1: Poly):
        if nb_rescales == 0:
            if p0 != p1:
                p1._mm_coeffs = p0._mm_coeffs.copy()
        else:
            if nb_rescales > 1:
                self._mm_inv_ntt_lvl(level, p0, buff)
                for i in range(nb_rescales):
                    self._mm_div_round_by_last_modulus_lvl(level - i, buff, buff)
                self._mm_ntt_lvl(level - nb_rescales, buff, p1)
            else:
                self._mm_div_round_by_last_modulus_ntt_lvl(level, p0, buff, p1)
    
    # poly_to_bigint reconstructs p1 and returns the result in an array of Int.
    # gap defines coefficients X^{i*gap} that will self reconstructed.
    # For example, if gap = 1, then all coefficients are reconstructed, while
    # if gap = 2 then only coefficients X^{2*i} are reconstructed.
    def poly_to_bigint(self, p1: Poly, gap: int, coeffs_bigint: list[big_uint]):
        self.poly_to_bigint_lvl(p1.level(), p1, gap, coeffs_bigint)
    
    # poly_to_bigint_lvl reconstructs p1 and returns the result in an array of Int.
    # gap defines coefficients X^{i*gap} that will self reconstructed.
    # For example, if gap = 1, then all coefficients are reconstructed, while
    # if gap = 2 then only coefficients X^{2*i} are reconstructed.
    def poly_to_bigint_lvl(self, level: int, p1: Poly, gap: int, coeffs_bigint: list[big_uint]):
        crt_reconstruction = list[big_uint](level + 1)
        modulus_bigint = self.modulus_at_level[level]

        for i in range(level + 1):
            qib = self.modulus[i].ext_to_bigint()
            crt_reconstruction.append(biguint_floordiv(modulus_bigint, qib))
            crt_reconstruction[i] *= biguint_mod(biguint_mod_inv(crt_reconstruction[i], qib), qib)

        i, j = 0, 0

        while j < self.n:
            coeffs_bigint[i] = big_uint(0)
            for k in range(level + 1):
                coeffs_bigint[i] += p1.coeffs[k][j].ext_to_bigint() * crt_reconstruction[k]
            coeffs_bigint[i] = biguint_mod(coeffs_bigint[i], modulus_bigint)
            i, j = i + 1, j + gap
    
    # PolyToBigintCenteredLvl reconstructs p1 and returns the result in an array of Int.
    # Coefficients are centered around Q/2
    # gap defines coefficients X^{i*gap} that will be reconstructed.
    # For example, if gap = 1, then all coefficients are reconstructed, while
    # if gap = 2 then only coefficients X^{2*i} are reconstructed.
    def poly_to_bigint_centered_lvl(self, level: int, p1: Poly, gap: int, coeffs_bigint: list[big_int]):
        crt_reconstruction = list[big_uint](level + 1)
        modulus_bigint = self.modulus_at_level[level]

        for i in range(level + 1):
            qib = self.modulus[i].ext_to_bigint()
            crt_reconstruction.append(biguint_floordiv(modulus_bigint, qib))
            crt_reconstruction[i] *= biguint_mod(biguint_mod_inv(crt_reconstruction[i], qib), qib)

        modulus_bigint_half = big_int(modulus_bigint >> 1)

        i, j = 0, 0
        while j < self.n:
            coeffs_bigint[i] = big_int(0)
            for k in range(level + 1):
                coeffs_bigint[i] += big_int(p1.coeffs[k][j].ext_to_bigint() * crt_reconstruction[k])
            coeffs_bigint[i] = bigint_mod(coeffs_bigint[i], big_int(modulus_bigint))

            # Centers the coefficients
            if coeffs_bigint[i] >= modulus_bigint_half:
                coeffs_bigint[i] -= big_int(modulus_bigint)

            i, j = i + 1, j + gap
    
    # SetCoefficientsBigintLvl sets the coefficients of p1 from an array of Int variables.
    def set_coefficients_bigint_lvl(self, level: int, coeffs: list[big_int], p1: Poly):
        for i in range(level + 1):
            qi_big_int = big_int(self.modulus[i].ext_to_bigint())
            for j, coeff in enumerate(coeffs):
                p1.coeffs[i][j] = u64(bigint_mod(coeff, qi_big_int).trunc_to_i64())


class BaseSampler:
    prng: prg.Random
    base_ring: Ring

    def __repr__(self) -> str:
        return f"""\tBase sampler:
                \tBase ring: {self.base_ring}
        """


class UniformSampler(Static[BaseSampler]):
    random_buffer_n: list[byte]

    def __init__(self, prng, base_ring, random_buffer_n):
        self.prng = prng
        self.base_ring = base_ring
        self.random_buffer_n = random_buffer_n
    
    def __bool__(self):
        return bool(len(self.random_buffer_n))
    
    def __repr__(self) -> str:
        return f"""\tUniformSampler:
                \t{self.get_base_sampler()}
        """
    
    def get_base_sampler(self):
        return BaseSampler(self.prng, self.base_ring)
    
    # ReadNew generates a new polynomial with coefficients following a uniform distribution over [0, Qi-1].
    # Polynomial is created at the max level.
    def _mm_read_new(self) -> Poly:
        pol = self.base_ring.new_poly()
        self._mm_read(pol)
        return pol
    
    # Read generates a new polynomial with coefficients following a uniform distribution over [0, Qi-1].
    def _mm_read(self, pol: Poly):
        self._mm_read_lvl(len(pol._mm_coeffs) - 1, pol)

    def _mm_read_lvl(self, level, pol):
        for j in range(level + 1):
            bitlen = int(self.base_ring.modulus[j].bitlen())
            _mm_qi = self.base_ring._mm_modulus[j]
            _mm_ptmp = pol._mm_coeffs[j]

            # Iterates for each modulus over each coefficient
            for i in range(self.base_ring._mm_n):
                # Samples an integer between [0, qi-1]
                _mm_random_uint = self.prng._mm_getrandbits(bitlen, T=u64xN)
                _mm_ptmp[i] = _mm_random_uint.sub_if(_mm_qi, _mm_random_uint.__ge__(_mm_qi))


# ModupParams stores the necessary parameters for RNS basis extension.
class ModupParams:
    # Parameters for basis extension from q to p
    # (q/Qi)^-1) (mod each Qi) (in Montgomery form)
    qoverqiinvqi: list[u64]
    _mm_qoverqiinvqi: list[u64xN]
    # q/qi (mod each Pj) (in Montgomery form)
    qoverqimodp: list[list[u64]]
    _mm_qoverqimodp: list[list[u64xN]]
	# q*v (mod each Pj) for v in [1,...,k] where k is the number of Pj moduli
    vtimesqmodp: list[list[u64]]


# Caution, returns the values in [0, 2q-1]
def _mm_mult_sum(
        v: u64xN, y: list[u64xN],
        pj_const: u64xN, q_inv_const: u64xN,
        vtimesqmodp: list[u64], qoverqimodp: list[u64xN]):
    r = u64xN(u64(0)).zext_double()
    # Accumulates the sum on uint128 and does a lazy montgomery reduction at the end
    for i in range(len(y)): r = r + y[i].zext_mul(qoverqimodp[i])

    h = (r.trunc_half() * q_inv_const).mulhi(pj_const)
    return r.shift_half().trunc_half() - h + pj_const + vtimesqmodp.shuffle(v)


def _mm_reconstruct_rns(
        p_row: list[u64xN], q: list[u64xN],
        q_inv: list[u64xN], qb_mont: list[u64xN]) -> tuple[u64xN, list[u64xN]]:
    v = f64xN(0.0)
    y = list[u64xN](len(p_row))

    for i in range(len(p_row)):
        y.append(_mm_mred(p_row[i], qb_mont[i], q[i], q_inv[i]))
        v = v + (y[i] / q[i])
    
    return v.to_u64(), y


# ModUpExact takes p1 mod q and switches its basis to P, returning the result on p2.
# Caution, returns the values in [0, 2q-1]
def _mm_mod_up_exact(p1: list[list[u64xN]], p2: list[list[u64xN]], ring_q: Ring, ring_p: Ring, params: ModupParams):
    # We loop over each coefficient and apply the basis extension
    for i in range(len(p1)):
        v, y = _mm_reconstruct_rns(
            p1[i], ring_q._mm_modulus, ring_q._mm_mred_params, params._mm_qoverqiinvqi)
        for j in range(len(p2[0])):
            p2[i][j] = _mm_mult_sum(
                v, y, ring_p._mm_modulus[j], ring_p._mm_mred_params[j], params.vtimesqmodp[j], params._mm_qoverqimodp[j])


class BasisExtender:
    ring_q: Ring
    ring_p: Ring
    params_q_to_p: list[ModupParams]
    params_p_to_q: list[ModupParams]
    _mm_mod_down_params_p_to_q: list[list[u64xN]]
    mod_down_params_q_to_p: list[list[u64]]

    buff_q: Poly
    buff_p: Poly

    def __repr__(self) -> str:
        return f"""\tBasisExtender:
                \tringQ: {self.ring_q}
                \tringP: {self.ring_p}
                \tbuffQ: {self.buff_q}
                \tbuffP: {self.buff_p}
        """
    
    # mod_up_p_to_q extends the RNS basis of a polynomial from P to PQ.
    # Given a polynomial with coefficients in basis {P0,P1....Plevel},
    # it extends its basis from {P0,P1....Plevel} to {Q0,Q1...Qj}
    def _mm_mod_up_p_to_q(self, level_p: int, level_q: int, pol_p: Poly, pol_q: Poly):
        _mm_reference = pol_q._mm_coeffs.slice_reference(0, level_q + 1).transpose()
        _mm_mod_up_exact(
            pol_p._mm_coeffs.slice_reference(0, level_p + 1).transpose(),
            _mm_reference,
            self.ring_p, self.ring_q, self.params_p_to_q[level_p])
        pol_q._mm_coeffs[:level_q] = _mm_reference.transpose()

    # ModDownQPtoQNTT reduces the basis of a polynomial.
    # Given a polynomial with coefficients in basis {Q0,Q1....Qi} and {P0,P1...Pj},
    # it reduces its basis from {Q0,Q1....Qi} and {P0,P1...Pj} to {Q0,Q1....Qi}
    # and does a rounded integer division of the result by P.
    # Inputs must self in the NTT domain.
    def _mm_mod_down_qp_to_q_ntt(self, level_q: int, level_p: int, p1_q: Poly, p1_p: Poly, p2_q: Poly):
        ring_q = self.ring_q
        ring_p = self.ring_p
        mod_down_params = self._mm_mod_down_params_p_to_q
        buff_p = self.buff_p
        buff_q = self.buff_q

        # First we get the P basis part of p1 out of the NTT domain
        ring_p._mm_inv_ntt_lazy_lvl(level_p, p1_p, buff_p)
        # Then we target this P basis of p1 and convert it to a q basis (at the "level" of p1) and copy it on the buffer.
        # The buffer is now the representation of the P basis of p1 but in basis q (at the "level" of p1)
        self._mm_mod_up_p_to_q(level_p, level_q, buff_p, buff_q)
        # First, we switch back the buffer CRT array back to the NTT domain
        ring_q._mm_ntt_lazy_lvl(level_q, buff_q, buff_q)
        
        # Finally, for each level of p1 (and the buffer since they now share the same basis) we compute p2 = (P^-1) * (p1 - buff) mod q
        for i in range(level_q + 1):
            # Then for each coefficient we compute (P^-1) * (p1[i][j] - buff[i][j]) mod qi
            _mm_sub_vec_and_mul_scalar_montgomery_two_qi_vec(
                buff_q._mm_coeffs[i], p1_q._mm_coeffs[i], p2_q._mm_coeffs[i],
                ring_q._mm_modulus[i] - mod_down_params[level_p][i],
                ring_q._mm_modulus[i], ring_q._mm_mred_params[i])

        # In total we do len(P) + len(q) NTT, which is optimal (linear in the number of moduli of P and q)

    # mod_down_qp_to_q reduces the basis of a polynomial.
    # Given a polynomial with coefficients in basis {Q0,Q1....Qlevel} and {P0,P1...Pj},
    # it reduces its basis from {Q0,Q1....Qlevel} and {P0,P1...Pj} to {Q0,Q1....Qlevel}
    # and does a rounded integer division of the result by P.
    def _mm_mod_down_qp_to_q(self, level_q: int, level_p: int, p1_q: Poly, p1_p: Poly, p2_q: Poly):
        ring_q = self.ring_q
        mod_down_params = self._mm_mod_down_params_p_to_q
        buff = self.buff_q

        # Then we target this P basis of p1 and convert it to a q basis (at the "level" of p1) and copy it on buff
        # buff is now the representation of the P basis of p1 but in basis q (at the "level" of p1)
        self._mm_mod_up_p_to_q(level_p, level_q, p1_p, buff)

        # Finally, for each level of p1 (and buff since they now share the same basis) we compute p2 = (P^-1) * (p1 - buff) mod q
        for i in range(level_q + 1):
            _mm_sub_vec_and_mul_scalar_montgomery_two_qi_vec(
                buff._mm_coeffs[i], p1_q._mm_coeffs[i], p2_q._mm_coeffs[i], ring_q._mm_modulus[i] - mod_down_params[level_p][i], ring_q._mm_modulus[i], ring_q._mm_mred_params[i])

        # In total we do len(P) + len(q) NTT, which is optimal (linear in the number of moduli of P and q)


# GaussianSampler keeps the state of a truncated Gaussian polynomial sampler.
class GaussianSampler(Static[BaseSampler]):
    sigma: float
    bound: int
    random_buffer_n: list[byte]
    ptr: u64

    def __repr__(self) -> str:
        return f"""\tGaussianSampler:
                \tSigma: {self.sigma}
                \tBound: {self.bound}
                \tPtr: {self.ptr}
        """
    
    # Read samples a truncated Gaussian polynomial on "pol" at the maximum level in the default ring, standard deviation and bound.
    def _mm_read(self, pol: Poly):
        self._mm_read_lvl(len(self.base_ring.modulus) - 1, pol)

    # # read_lvl samples a truncated Gaussian polynomial at the provided level, in the default ring, standard deviation and bound.
    # def old_read_lvl(self, level, pol):
    #     self.old_read_lvl(level, pol, self.base_ring, self.sigma, self.bound)
    
    # def old_read_lvl(self, level, pol, ring, sigma, bound):
    #     coeff_int, sign = u64(0), u64(0)
    #     modulus = ring.modulus[:level + 1]

    #     for i in range(ring.n):
    #         while True:
    #             coeff_flo, sign = self.norm_float_64()
    #             coeff_int = u64(int(coeff_flo * sigma + 0.5))
    #             if coeff_int <= u64(bound): break

    #         for j, qi in enumerate(modulus):
    #             pol.coeffs[j][i] = u64(1)  # TODO: Revert: (coeff_int * sign) | (qi - coeff_int) * (sign ^ u64(1))
    
    def _mm_read_lvl(self, level, pol):
        # TODO: Temp method (see old_read_lvl above)
        _mm_ones = u64xN(u64(1))
        for i in range(level + 1):
            for j in range(self.base_ring._mm_n):
                pol._mm_coeffs[i][j] = _mm_ones
        
    # NormFloat64 returns a normally distributed float64 in
    # the range [-math.MaxFloat64, +math.MaxFloat64], bounds included,
    # with standard normal distribution (mean = 0, stddev = 1).
    # To produce a different normal distribution, callers can
    # adjust the output using:
    #  sample = NormFloat64() * desiredStdDev + desiredMean
    def norm_float_64(self):
        r = self.prng.gauss(mu=0, sigma=1)
        return abs(r), u64(r < 0)
    
    # read_and_add_lvl samples a truncated Gaussian polynomial at the given level for the receiver's default standard deviation and bound and adds it on "pol".
    def _mm_read_and_add_lvl(self, level: int, pol: Poly):
        self._mm_read_and_add_from_dist_lvl(level, pol, self.base_ring, self.sigma, self.bound)
    
    # read_and_add_lvl samples a truncated Gaussian polynomial at the given level for the receiver's default standard deviation and bound and adds it on "pol".
    def _mm_read_and_add_lvl(self, level: int, pol: Poly):
        self._mm_read_and_add_from_dist_lvl(level, pol, self.base_ring, self.sigma, self.bound)
    
    # read_and_add_from_dist_lvl samples a truncated Gaussian polynomial at the given level in the provided ring, standard deviation and bound and adds it on "pol".
    def _mm_read_and_add_from_dist_lvl(self, level: int, pol: Poly, ring: Ring, sigma: float, bound: int):
        modulus = ring.modulus[:level + 1]
        coeff_flo, sign, coeff_int = 0.0, u64(0), u64(0)

        for j, qi in enumerate(modulus):
            while True:
                coeff_flo, sign = self.norm_float_64()
                coeff_int = u64(int(coeff_flo * sigma + 0.5))
                if coeff_int <= u64(bound): break

            for i in range(ring._mm_n):
                _mm_noise = u64xN((coeff_int * sign) | (qi - coeff_int) * (sign ^ u64(1)))
                pol._mm_coeffs[j][i] = _mm_cred(pol._mm_coeffs[j][i] + _mm_noise, ring._mm_modulus[j])

# TernarySampler keeps the state of a polynomial sampler in the ternary distribution.
class TernarySampler(Static[BaseSampler]):
    matrix_proba: list[list[u8]]  # (2)(RING_PRECISION - 1)
    matrix_values: list[list[u64]]  # ()(3)
    p: float
    hw: int
    sample_name: str

    def __repr__(self) -> str:
        return f"""\tTernarySampler:
                \tmatrixValues: {self.matrix_values}
                \tp: {self.p}
                \thw: {self.hw}
                \tsample_name: {self.sample_name}
        """
    
    def sample_sparse(self, lvl: int, pol: Poly):
        m, n = pol._mm_coeffs.shape()
        coeffs = zeros(m, n * SIMD_LANE_SIZE, TP=u64)
        
        if self.hw > self.base_ring.n:
            self.hw = self.base_ring.n
    
        index = arange(self.base_ring.n, TP=int)

        for i in range(self.hw):
            bit_len = int((self.base_ring.n - i).bitlen()) - 1  # rejection sampling of a random variable between [0, len(index)]
            j = self.prng.getrandbits(bit_len)

            coeff = self.prng.getrandbits(1)  # random binary digit [0, 1] from the random bytes (0 = 1, 1 = -1)
            for k in range(lvl + 1):
                coeffs[k][index[j]] = self.matrix_values[k][coeff + 1]

            # Remove the element in position j of the slice (order not preserved)
            index[j] = index.pop()

        return coeffs
    
    def initialize_matrix(self, montgomery):
        self.matrix_values = zeros(len(self.base_ring.modulus), 3, TP=u64)

        # [0] = 0
        # [1] = 1 * 2^64 mod qi
        # [2] = (qi - 1) * 2^64 mod qi

        for i, Qi in enumerate(self.base_ring.modulus):
            self.matrix_values[i][0] = u64(0)

            if montgomery:
                self.matrix_values[i][1] = mform(u64(1), Qi, self.base_ring.bred_params[i])
                self.matrix_values[i][2] = mform(Qi - u64(1), Qi, self.base_ring.bred_params[i])
            else:
                self.matrix_values[i][1] = u64(1)
                self.matrix_values[i][2] = Qi - u64(1)
    
    def _mm_sample(self, lvl: int, r: Poly):
        if self.sample_name == 'samplesparse':
            r._mm_coeffs = self.sample_sparse(lvl, r).vectorize(SIMD_LANE_SIZE)
        else: raise NotImplementedError()

    # ReadLvl samples a polynomial into pol at the speciefied level.
    def _mm_read_lvl(self, lvl: int, pol: Poly):
        self._mm_sample(lvl, pol)
    
    # ReadLvlNew allocates and samples a polynomial at the speficied level.
    def _mm_read_lvl_new(self, lvl: int) -> Poly:
        pol = self.base_ring.new_poly_lvl(lvl)
        self._mm_read_lvl(lvl, pol)
        return pol

    # read samples a polynomial into pol.
    def _mm_read(self, pol):
        self._mm_read_lvl(len(self.base_ring.modulus) - 1, pol)

    # ReadNew allocates and samples a polynomial at the max level.
    def _mm_read_new(self) -> Poly:
        pol = self.base_ring.new_poly()
        self._mm_read(pol)
        return pol


# NewRingWithCustomNTT creates a new RNS Ring with degree n and coefficient moduli moduli with user-defined NTT transform and primitive Nth root of unity.
# moduli should self a non-empty []u64 with distinct prime elements. All moduli must also self equal to 1 modulo the root of unity.
# n must self a power of two larger than 8. An error is returned with a nil *Ring in the case of non NTT-enabling parameters.
def new_ring_with_custom_ntt(n, moduli, ntt, nth_root):
    r = Ring()
    r.set_parameters(n, moduli)
    r.set_number_theoretic_transformer(ntt)
    r.gen_ntt_params(u64(nth_root))

    return r


def new_ring_from_type(n, moduli, ring_type):
    if ring_type == StandardRing:
        return new_ring_with_custom_ntt(n, moduli, NumberTheoreticTransformerStandard().name, 2 * n)
    if ring_type == ConjugateInvariantRing:
        return new_ring_with_custom_ntt(n, moduli, NumberTheoreticTransformerConjugateInvariant().name, 4 * n)
    raise ValueError("invalid ring type")


# GenerateNTTPrimesQ generates "levels" different nth_root NTT-friendly
# primes starting from 2**LogQ and alternating between upward and downward.
def generate_ntt_primes_q(logq, nth_root, levels):
    primes = []
    q_pow2 = u64(1 << logq)
    next_prime = q_pow2 + u64(1)
    previous_prime = q_pow2 + u64(1)
    checkfornextprime = True
    checkforpreviousprime = True

    while True:
        if not (checkfornextprime or checkforpreviousprime):
            raise ValueError("generateNTTPrimesQ error: cannot generate enough primes for the given parameters")

        if checkfornextprime:
            if next_prime > u64(0xffffffffffffffff) - u64(nth_root):
                checkfornextprime = False
            else:
                if utils.is_prime(next_prime):
                    primes.append(next_prime)
                    if len(primes) == levels: return primes
                next_prime += u64(nth_root)

        if checkforpreviousprime:
            if previous_prime < u64(nth_root):
                checkforpreviousprime = False
            else:
                previous_prime -= u64(nth_root)
				
                if utils.is_prime(previous_prime):
                    primes.append(previous_prime)
                    if len(primes) == levels: return primes


# GenerateNTTPrimesP generates "levels" different nth_root NTT-friendly
# primes starting from 2**LogP and downward.
# Special case were primes close to 2^{LogP} but with a smaller bit-size than LogP are sought.
def generate_ntt_primes_p(logP, nth_root, n):
    primes = []
    p_pow2 = u64(1 << logP)
    x = p_pow2 + u64(1)

    while True:
        # We start by subtracting 2N to ensure that the prime bit-length is smaller than LogP
        if x > u64(nth_root):
            x -= u64(nth_root)
            if utils.is_prime(x):
                primes.append(x)
                if len(primes) == n: return primes
        else: raise ValueError("generateNTTPrimesP error: cannot generate enough primes for the given parameters")


# GenerateNTTPrimes generates n nth_root NTT friendly primes given logq = size of the primes.
# It will return all the appropriate primes, up to the number of n, with the
# best available deviation from the base power of 2 for the given n.
def generate_ntt_primes(logq, nth_root, n):
    assert logq < 62, "logq must self between 1 and 61"

    if logq == 61: return generate_ntt_primes_p(logq, nth_root, n)
    return generate_ntt_primes_q(logq, nth_root, n)


# NewUniformSampler creates a new instance of UniformSampler from a PRNG and ring definition.
def new_uniform_sampler(prng, base_ring):
    return UniformSampler(
        prng=prng,
        base_ring=base_ring,
        random_buffer_n=zeros_vec(base_ring.n, TP=byte))


# GenModUpParams generates the ModupParams for basis extension from q to p and p to q.
def gen_mod_up_params(q, p):
    bred_params_q = list[list[u64]](len(q))
    mred_params_q = list[u64](len(q))
    bred_params_p = list[list[u64]](len(p))
    mred_params_p = list[u64](len(p))

    for qi in q:
        bred_params_q.append(bred_params(qi))
        mred_params_q.append(mred_params(qi))

    for pi in p:
        bred_params_p.append(bred_params(pi))
        mred_params_p.append(mred_params(pi))

    qoverqiinvqi = list[u64](len(q))
    _mm_qoverqiinvqi = list[u64xN](len(q))
    qoverqimodp = list[list[u64]](len(q))
    _mm_qoverqimodp = list[list[u64xN]](len(q))

    for i, qi in enumerate(q):
        qi_star = mform(u64(1), qi, bred_params_q[i])

        for j in range(len(q)):
            if j != i: qi_star = mred(qi_star, mform(q[j], qi, bred_params_q[i]), qi, mred_params_q[i])

        # (q/Qi)^-1) * r (mod Qi) (in Montgomery form)
        qoverqiinvqi.append(mod_exp_montgomery(qi_star, int(qi - u64(2)), qi, mred_params_q[i], bred_params_q[i]))
        _mm_qoverqiinvqi.append(u64xN(qoverqiinvqi[i]))

        qoverqimodp_row = list[u64](len(p))
        _mm_qoverqimodp_row = list[u64xN](len(p))
        for j, pj in enumerate(p):
            # (q/qi * r) (mod Pj) (in Montgomery form)
            qi_star = u64(1)
            for u in range(len(q)):
                if u != i: qi_star = mred(qi_star, mform(q[u], pj, bred_params_p[j]), pj, mred_params_p[j])

            qoverqimodp_row.append(mform(qi_star, pj, bred_params_p[j]))
            _mm_qoverqimodp_row.append(u64xN(qoverqimodp_row[j]))
        qoverqimodp.append(qoverqimodp_row)
        _mm_qoverqimodp.append(_mm_qoverqimodp_row)

    vtimesqmodp = list[list[u64]](len(p))
    for j, pj in enumerate(p):
        vtimesqmodp.append(list[u64](len(q) + 1))
		# Correction Term (v*q) mod each Pj

        q_mod_pi = u64(1)
        for qi in q: q_mod_pi = mred(q_mod_pi, mform(qi, pj, bred_params_p[j]), pj, mred_params_p[j])

        v = pj - q_mod_pi
        vtimesqmodp[j].append(u64(0))
        for i in range(1, len(q) + 1): vtimesqmodp[j].append(cred(vtimesqmodp[j][i - 1] + v, pj))
    
    return ModupParams(
        qoverqiinvqi=qoverqiinvqi,
        _mm_qoverqiinvqi=_mm_qoverqiinvqi,
        qoverqimodp=qoverqimodp.transpose(),
        _mm_qoverqimodp=_mm_qoverqimodp.transpose(),
        vtimesqmodp=vtimesqmodp)


def gen_mod_down_params(ring_q, ring_p):
    params = list[list[u64]](len(ring_p.modulus))

    bred_params = ring_q.bred_params
    mred_params = ring_q.mred_params

    for j in range(len(ring_p.modulus)):
        params.append(list[u64](len(ring_q.modulus)))
        for i, qi in enumerate(ring_q.modulus):
            params[j].append(mod_exp_u64(ring_p.modulus[j], qi - u64(2), qi))
            params[j][i] = mform(params[j][i], qi, bred_params[i])

            if j > 0: params[j][i] = mred(params[j][i], params[j - 1][i], qi, mred_params[i])

    return params


def new_basis_extender(ring_q, ring_p):
    new_params = BasisExtender()

    new_params.ring_q = ring_q
    new_params.ring_p = ring_p

    new_params.params_q_to_p = list[ModupParams](len(ring_q.modulus))
    for i in range(len(ring_q.modulus)):
        new_params.params_q_to_p.append(gen_mod_up_params(ring_q.modulus[:i + 1], ring_p.modulus))

    new_params.params_p_to_q = list[ModupParams](len(ring_p.modulus))
    for i in range(len(ring_p.modulus)):
        new_params.params_p_to_q.append(gen_mod_up_params(ring_p.modulus[:i + 1], ring_q.modulus))

    mod_down_params_p_to_q = gen_mod_down_params(ring_q, ring_p)
    new_params._mm_mod_down_params_p_to_q = [[u64xN(e) for e in row] for row in mod_down_params_p_to_q]
    new_params.mod_down_params_q_to_p = gen_mod_down_params(ring_p, ring_q)

    new_params.buff_q = ring_q.new_poly()
    new_params.buff_p = ring_p.new_poly()

    return new_params


# NewGaussianSampler creates a new instance of GaussianSampler from a PRNG, a ring definition and the truncated
# Gaussian distribution parameters. Sigma is the desired standard deviation and bound is the maximum coefficient norm in absolute
# value.
def new_gaussian_sampler(prng, base_ring, sigma, bound):
    return GaussianSampler(
        prng=prng,
        random_buffer_n=[byte(0) for _ in range(1024)],
        ptr=u64(0),
        base_ring=base_ring,
        sigma=sigma,
        bound=bound)


# NewTernarySamplerWithHammingWeight creates a new instance of a fixed-hamming-weight TernarySampler from a PRNG, the ring definition and the desired
# hamming weight for the output polynomials. If "montgomery" is set to true, polynomials read from this sampler
# are in Montgomery form.
def new_ternary_sampler_with_hamming_weight(prng, base_ring, hw, montgomery):
    ternary_sampler = TernarySampler(
        base_ring=base_ring,
        prng=prng,
        hw=hw,
        sample_name="samplesparse")
    ternary_sampler.initialize_matrix(montgomery)
    return ternary_sampler


# CopyValuesLvl copies the values of p0 on p1, up to level+1 moduli.
def copy_values_lvl(level, p0, p1):
    for i in range(level + 1): p1.coeffs[i] = copy(p0.coeffs[i])

# CopyLvl copies the coefficients of p0 on p1 within the given Ring.
# Copies for up to level+1 moduli.
# Expects the degree of both polynomials to self identical.
# Transfers the is_ntt and is_mform flags.
def copy_lvl(level, p0, p1):
	copy_values_lvl(level, p0, p1)
	p1.is_ntt = p0.is_ntt
	p1.is_mform = p0.is_mform


# Decomposer is a structure that stores the parameters of the arbitrary decomposer.
# This decomposer takes a p(x)_Q (in basis q) and returns p(x) mod qi in basis QP, where
# qi = prod(Q_i) for 0<=i<=L, where L is the number of factors in p.
class Decomposer:
    ring_q: Ring
    ring_p: Ring
    mod_up_params: list[list[list[ModupParams]]]

    # decompose_and_split decomposes a polynomial p(x) in basis q, reduces it modulo qi, and returns
    # the result in basis QP separately.
    def _mm_decompose_and_split(
            self, level_q: int, level_p: int, nb_pi: int,
            decomp_rns: int, p0_q: Poly, p1_q: Poly, p1_p: Poly):
        ring_q = self.ring_q
        ring_p = self.ring_p
        lvl_q_start = decomp_rns * nb_pi

        decomp_lvl = 0
        if level_q > nb_pi * (decomp_rns + 1) - 1: decomp_lvl = nb_pi - 2
        else: decomp_lvl = (level_q % nb_pi) - 1

        # First we check if the vector can simply by copying and rearranging elements (the case where no reconstruction is needed)
        if decomp_lvl == -1:
            for j in range(level_q + 1): p1_q._mm_coeffs[j] = copy(p0_q._mm_coeffs[lvl_q_start])
            for j in range(level_p + 1): p1_p._mm_coeffs[j] = copy(p0_q._mm_coeffs[lvl_q_start])
            # Otherwise, we apply a fast exact base conversion for the reconstruction
        else:
            p0_idx_st = decomp_rns * nb_pi
            p0_idx_ed = p0_idx_st + nb_pi

            if p0_idx_ed > level_q + 1: p0_idx_ed = level_q + 1
            params = self.mod_up_params[nb_pi - 2][decomp_rns][decomp_lvl]

            q = ring_q._mm_modulus
            p = ring_p._mm_modulus
            mred_params_q = ring_q._mm_mred_params
            mred_params_p = ring_p._mm_mred_params
            vtimesqmodp = params.vtimesqmodp
            qoverqimodp = params._mm_qoverqimodp

            _mm_p0_q_coeffs = p0_q._mm_coeffs[:level_q + 1].transpose()
            _mm_p1_q_coeffs = p1_q._mm_coeffs[:level_q + 1].transpose()
            _mm_p1_p_coeffs = p1_p._mm_coeffs[:level_p + 1].transpose()
            
            y = [u64xN(u64(0)) for _ in range(decomp_lvl + 2)]
            # We loop over each coefficient and apply the basis extension
            for x in range(len(_mm_p0_q_coeffs)):
                i, j = 0, lvl_q_start
                vf = f64xN(0.0)

                # Coefficients to self decomposed
                while i < decomp_lvl + 2:
                    # For the coefficients to self decomposed, we can simply copy them
                    _mm_p1_q_coeffs[x][j] = _mm_p0_q_coeffs[x][j]
                    y[i] = _mm_mred(_mm_p0_q_coeffs[x][j], params._mm_qoverqiinvqi[i], q[j], mred_params_q[j])
                    # Computation of the correction term v * q%pi
                    vf = vf + y[i] / q[j]
                    i, j = i + 1, j + 1

                # Index of the correction term
                v = vf.to_u64()
 
                # Coefficients of index smaller than the ones to self decomposed
                for j in range(p0_idx_st):
                    _mm_p1_q_coeffs[x][j] = _mm_mult_sum(v, y, q[j], mred_params_q[j], vtimesqmodp[j], qoverqimodp[j])

                # Coefficients of index greater than the ones to self decomposed
                for j in range(p0_idx_ed, level_q + 1):
                    _mm_p1_q_coeffs[x][j] = _mm_mult_sum(v, y, q[j], mred_params_q[j], vtimesqmodp[j], qoverqimodp[j])

                # Coefficients of the special primes Pi
                j, u = 0, len(ring_q.modulus)
                while j < level_p + 1:
                    _mm_p1_p_coeffs[x][j] = _mm_mult_sum(v, y, p[j], mred_params_p[j], vtimesqmodp[u], qoverqimodp[u])
                    j, u = j + 1, u + 1

            _mm_p0_q_coeffs = _mm_p0_q_coeffs.transpose()
            _mm_p1_q_coeffs = _mm_p1_q_coeffs.transpose()
            # Copies the coefficients of polynomials mod the RNS decomposition
            for i in range(p0_idx_st, p0_idx_ed): _mm_p1_q_coeffs[i] = copy(_mm_p0_q_coeffs[i])

            p1_q._mm_coeffs[:level_q + 1] = _mm_p1_q_coeffs
            p1_p._mm_coeffs[:level_p + 1] = _mm_p1_p_coeffs.transpose()
        

# NewDecomposer creates a new Decomposer.
def new_decomposer(ring_q: Ring, ring_p: Ring):
    decomposer = Decomposer()

    decomposer.ring_q = ring_q
    decomposer.ring_p = ring_p

    q = ring_q.modulus

    decomposer.mod_up_params = list[list[list[ModupParams]]](len(ring_p.modulus) - 1)

    for lvl_p in range(len(ring_p.modulus) - 1):
        p = ring_p.modulus[:lvl_p + 2]

        nb_pi = len(p)
        decomp_rns = (len(q) + nb_pi - 1) // nb_pi

        xnb_pi = [nb_pi for _ in range(decomp_rns)]

        if len(q) % nb_pi != 0: xnb_pi[decomp_rns - 1] = len(q) % nb_pi

        decomposer.mod_up_params.append(list[list[ModupParams]](decomp_rns))

        # Create mod_up_params for each possible combination of [Qi,Pj] according to xnb_pi
        for i in range(decomp_rns):
            decomposer.mod_up_params[lvl_p].append(list[ModupParams](xnb_pi[i] - 1))

            for j in range(xnb_pi[i] - 1):
                q_i = list[u64](j + 2)
                p_i = list[u64](len(q) + len(p))

                for k in range(j + 2): q_i.append(q[i * nb_pi + k])

                p_i = copy(q)

                for k  in range(len(q), len(q) + len(p)):
                    p_i.append(p[k - len(q)])

                decomposer.mod_up_params[lvl_p][i].append(gen_mod_up_params(q_i, p_i))

    return decomposer
