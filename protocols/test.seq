from sequre.mpc.env import MPCEnv
from ..tests.test_all import test_all
from ..tests.test_unit import test_unit
from ..tests.benchmark import benchmark


def all_test_protocol[TP](mpc: MPCEnv[TP], test_run: bool):
    if test_run:
        benchmark(mpc)
    else:
        test_all[TP](mpc)


def lite_test_protocol[TP](mpc: MPCEnv[TP], test_run: bool):
    if test_run:
        benchmark(mpc)
    else:
        test_unit[TP](mpc)
